<lb n="13" xml:id="Mf6r.13"/><l n="M_II,12_0003"><hi rend="capitalsize1">W</hi>ant dune waers te rade niet</l>
<lb n="14" xml:id="Mf6r.14"/><l n="M_II,12_0004"><hi rend="capitalsize1">D</hi>aer hi di sciep ende leuen hiet</l>
</lg>
<lg type="disticha" n="II,13">
<lb n="15" xml:id="Mf6r.15"/><l n="M_II,13_0001"><hi rend="capitalsize1">I</hi>c verbiede di nidichede</l>
<lb n="16" xml:id="Mf6r.16"/><l n="M_II,13_0002"><hi rend="capitalsize1">W</hi>ant soe heuet quade sede</l>
<lb n="17" xml:id="Mf6r.17"/><l n="M_II,13_0003"><hi rend="capitalsize1">W</hi>iese draghet god weet</l>
<lb n="18" xml:id="Mf6r.18"/><l n="M_II,13_0004"><hi rend="capitalsize1">H</hi>em comter of dat hem es leet</l>
</lg>
<lg type="disticha" n="II,14">
<lb n="19" xml:id="Mf6r.19"/><l n="M_II,14_0001"><hi rend="capitalsize1">D</hi>u sult hebben harden moet</l>
<lb n="20" xml:id="Mf6r.20"/><l n="M_II,14_0002"><hi rend="capitalsize1">W</hi>anneer men di onrecht doet</l>


<pb xml:id="M.f6v" n="6v"/>
<lb n="1" xml:id="Mf6v.1"/><l n="M_II,14_0003"><hi rend="capitalsize1">W</hi>ant het sit wel den man</l>
<lb n="2" xml:id="Mf6v.2"/><l n="M_II,14_0004"><hi rend="capitalsize1">D</hi>atti alsoe doen can</l>
</lg>
<lg type="disticha" n="independent stanza">
<lb n="3" xml:id="Mf6v.3"/><l n="M_independent stanza_0001"><hi rend="capitalsize1">S</hi>one dits die beste scat</l>
<lb n="4" xml:id="Mf6v.4"/><l n="M_independent stanza_0002"><hi rend="capitalsize1">D</hi>ie ic hebbe <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> om dat</l>
<lb n="5" xml:id="Mf6v.5"/><l n="M_independent stanza_0003"><hi rend="capitalsize1">H</hi>eetic ende beuelic dy</l>
<lb n="6" xml:id="Mf6v.6"/><l n="M_independent stanza_0004"><hi rend="capitalsize1">D</hi>at hi altoos metti sy</l>
</lg>
</p>
</body>
</text>


