        <group><pb xml:id="M.f1r" n="1r"/>


<text n="prologue" xml:id="prologue">
<body>
<p><lg type="disticha" n="prologue">
<lb n="1" xml:id="Mf1r.1"/><l n="M_prologue_0001"><hi rend="capitalsize3">D</hi>Ie ghene die in haren sinne</l>
<lb n="2" xml:id="Mf1r.2"/><l n="M_prologue_0002"><hi rend="capitalsize1">D</hi>raghen waerlike minne</l>
<lb n="3" xml:id="Mf1r.3"/><l n="M_prologue_0003"><hi rend="capitalsize1">S</hi>i maker of riim <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> liet</l>
<lb n="4" xml:id="Mf1r.4"/><l n="M_prologue_0004"><hi rend="capitalsize1">D</hi>er minnen sone draghic niet</l>
<lb n="5" xml:id="Mf1r.5"/><l n="M_prologue_0005"><hi rend="capitalsize1">D</hi>er of eist mi al vergaen</l>
<lb n="6" xml:id="Mf1r.6"/><l n="M_prologue_0006"><hi rend="capitalsize1">D</hi>atter minne mach bestaen</l>
<lb n="7" xml:id="Mf1r.7"/><l n="M_prologue_0007"><hi rend="capitalsize1">D</hi>ie wile als ic die minne drouch</l>
<lb n="8" xml:id="Mf1r.8"/><l n="M_prologue_0008"><hi rend="capitalsize1">A</hi>ls ic die minne sach ic louch</l>
<lb n="9" xml:id="Mf1r.9"/><l n="M_prologue_0009"><hi rend="capitalsize1">N</hi>u haticse al in mine sinne</l>
<lb n="10" xml:id="Mf1r.10"/><l n="M_prologue_0010"><hi rend="capitalsize1">D</hi>ie minne draghen entie minne</l>
<lb n="11" xml:id="Mf1r.11"/><l n="M_prologue_0011"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> hebbe ghekeert minen moet</l>
<lb n="12" xml:id="Mf1r.12"/><l n="M_prologue_0012"><hi rend="capitalsize1">A</hi>n die ghene die siin vroet</l>
<lb n="13" xml:id="Mf1r.13"/><l n="M_prologue_0013"><hi rend="capitalsize1">E</hi>er ic dit dede seide <num type="roman">.j.</num> miin vrient</l>
<lb n="14" xml:id="Mf1r.14"/><l n="M_prologue_0014"><hi rend="capitalsize1">G</hi>heselle die ter wijsheit dient</l>
<lb n="15" xml:id="Mf1r.15"/><l n="M_prologue_0015"><hi rend="capitalsize1">D</hi>attijs betren loon ontfaet</l>
<lb n="16" xml:id="Mf1r.16"/><l n="M_prologue_0016"><hi rend="capitalsize1">D</hi>an die ter <choice><abbr>mi<g ref="#bar"/>nen</abbr><expan>mi<ex>n</ex>nen</expan></choice> dienste staet</l>
<lb n="17" xml:id="Mf1r.17"/><l n="M_prologue_0017"><hi rend="capitalsize1">H</hi>ier om hebbic dies begonnen</l>
<lb n="18" xml:id="Mf1r.18"/><l n="M_prologue_0018"><hi rend="capitalsize1">S</hi>i hebben onrecht dies mi verionnen</l>
<lb n="19" xml:id="Mf1r.19"/><l n="M_prologue_0019"><hi rend="capitalsize2">N</hi>v bidt alle met mi te gode</l>
<lb n="20" xml:id="Mf1r.20"/><l n="M_prologue_0020"><hi rend="capitalsize1">D</hi>at hi mi sende sine bode</l>


<pb xml:id="M.f1v" n="1v"/>
<lb n="1" xml:id="Mf1v.1"/><l n="M_prologue_0021"><hi rend="capitalsize1">D</hi>ie mi wise ende leere</l>
<lb n="2" xml:id="Mf1v.2"/><l n="M_prologue_0022"><hi rend="capitalsize1">H</hi>oe ic mi ter wijsheit keere</l>
<lb n="3" xml:id="Mf1v.3"/><l n="M_prologue_0023"><hi rend="capitalsize1">A</hi>lle die vroet willen wesen</l>
<lb n="4" xml:id="Mf1v.4"/><l n="M_prologue_0024"><hi rend="capitalsize1">E</hi>en bouc es dattie clercken lesen</l>
<lb n="5" xml:id="Mf1v.5"/><l n="M_prologue_0025"><hi rend="capitalsize1">A</hi>lsi eerst ter scolen gaen</l>
<lb n="6" xml:id="Mf1v.6"/><l n="M_prologue_0026"><hi rend="capitalsize1">D</hi>ie hem wijsheit doet verstaen</l>
<lb n="7" xml:id="Mf1v.7"/><l n="M_prologue_0027"><hi rend="capitalsize1">V</hi>ele meer dan eenich doet</l>
<lb n="8" xml:id="Mf1v.8"/><l n="M_prologue_0028"><hi rend="capitalsize1">H</hi>ier om peinsic in minen moet</l>
<lb n="9" xml:id="Mf1v.9"/><l n="M_prologue_0029"><hi rend="capitalsize1">D</hi>at icker v bi wille leeren</l>
<lb n="10" xml:id="Mf1v.10"/><l n="M_prologue_0030"><hi rend="capitalsize1">H</hi>oe ghi v herte sult bekeeren</l>
<lb n="11" xml:id="Mf1v.11"/><l n="M_prologue_0031"><hi rend="capitalsize1">T</hi>er wijsheit nv mercket dan</l>
<lb n="12" xml:id="Mf1v.12"/><l n="M_prologue_0032"><hi rend="capitalsize1">W</hi>iene maecte het woonde <num type="roman">.j.</num> man</l>
<lb n="13" xml:id="Mf1v.13"/><l n="M_prologue_0033"><hi rend="capitalsize1">T</hi>ote rome wilen eer</l>
<lb n="14" xml:id="Mf1v.14"/><l n="M_prologue_0034"><hi rend="capitalsize1">D</hi>ie des wijsdoms wiste meer</l>
<lb n="15" xml:id="Mf1v.15"/><l n="M_prologue_0035"><hi rend="capitalsize1">D</hi>an eenich die leuet nv</l>
<lb n="16" xml:id="Mf1v.16"/><l n="M_prologue_0036"><hi rend="capitalsize1">H</hi>i hiet cathoen dat segghic v</l>
<lb n="17" xml:id="Mf1v.17"/><l n="M_prologue_0037"><hi rend="capitalsize1">H</hi>i hadde <num type="roman">.j.</num> kint dat hiet also</l>
<lb n="18" xml:id="Mf1v.18"/><l n="M_prologue_0038"><hi rend="capitalsize1">A</hi>ls hi dede des was hi vro</l>
<lb n="19" xml:id="Mf1v.19"/><l n="M_prologue_0039"><hi rend="capitalsize1">A</hi>lst metten kinde was vergaen</l>
<lb n="20" xml:id="Mf1v.20"/><l n="M_prologue_0040"><hi rend="capitalsize1">D</hi>at mochte <choice><abbr>onthoude<g ref="#bar"/></abbr><expan>onthoude<ex>n</ex></expan></choice> <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> verstaen</l>


<pb xml:id="M.f2r" n="2r"/>
<lb n="1" xml:id="Mf2r.1"/><l n="M_prologue_0041"><hi rend="capitalsize1">R</hi>iep hijt <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> sprac te hem</l>
<lb n="2" xml:id="Mf2r.2"/><l n="M_prologue_0042"><hi rend="capitalsize1">M</hi>en seit sone dat ic bem</l>
<lb n="3" xml:id="Mf2r.3"/><l n="M_prologue_0043"><hi rend="capitalsize1">W</hi>ijs <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> vroet nv mercket dan</l>
<lb n="4" xml:id="Mf2r.4"/><l n="M_prologue_0044"><hi rend="capitalsize1">I</hi>c sal v leeren dat ic can</l>
<lb n="5" xml:id="Mf2r.5"/><l n="M_prologue_0045"><hi rend="capitalsize1">H</hi>i seide <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> begonste aldus</l>
<lb n="6" xml:id="Mf2r.6"/><l n="M_prologue_0046"><hi rend="capitalsize1">S</hi>i deus est animus</l>
</lg>
</p>
</body>
</text>


<text n="disticha" xml:id="disticha">
<body>
<p><lg type="disticha" n="I,01">
<lb n="7" xml:id="Mf2r.7"/><l n="M_I,01_0001"><hi rend="capitalsize2">N</hi>v mercke wat ic di ghebiede</l>
<lb n="8" xml:id="Mf2r.8"/><l n="M_I,01_0002"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> wat es dat ic dy bediede</l>
<lb n="9" xml:id="Mf2r.9"/><l n="M_I,01_0003"><hi rend="capitalsize1">D</hi>ats dattu di daer an sult keeren</l>
<lb n="10" xml:id="Mf2r.10"/><l n="M_I,01_0004"><hi rend="capitalsize1">D</hi>attu gode emmer sult eeren</l>
</lg>
<lg type="disticha" n="I,02">
<lb n="11" xml:id="Mf2r.11"/><l n="M_I,02_0001"><hi rend="capitalsize1">D</hi>u sult smorghens vro up staen</l>
<lb n="12" xml:id="Mf2r.12"/><l n="M_I,02_0002"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> om diin bederue gaen</l>
<lb n="13" xml:id="Mf2r.13"/><l n="M_I,02_0003"><hi rend="capitalsize1">M</hi>en seit die te langhe slaept</l>
<lb n="14" xml:id="Mf2r.14"/><l n="M_I,02_0004"><hi rend="capitalsize1">D</hi>at hem de slaep onduchtich maect</l>
</lg>
<lg type="disticha" n="I,03">
<lb n="15" xml:id="Mf2r.15"/><l n="M_I,03_0001"><hi rend="capitalsize1">W</hi>achtu <choice><abbr>datme<g ref="#bar"/></abbr><expan>datme<ex>n</ex></expan></choice> niet en seghet</l>
<lb n="16" xml:id="Mf2r.16"/><l n="M_I,03_0002"><hi rend="capitalsize1">D</hi>at vele talen an di leghet</l>
<lb n="17" xml:id="Mf2r.17"/><l n="M_I,03_0003"><hi rend="capitalsize1">M</hi>en seit menigherande tale</l>
<lb n="18" xml:id="Mf2r.18"/><l n="M_I,03_0004"><hi rend="capitalsize1">E</hi>n sit niemens goeders wale</l>
</lg>
<lg type="disticha" n="I,04">
<lb n="19" xml:id="Mf2r.19"/><l n="M_I,04_0001"><hi rend="capitalsize1">S</hi>one doet alsoe die vroede doet</l>
<lb n="20" xml:id="Mf2r.20"/><l n="M_I,04_0002"><hi rend="capitalsize1">D</hi>ie heuet ghestaden moet</l>


<pb xml:id="M.f2v" n="2v"/>
<lb n="1" xml:id="Mf2v.1"/><l n="M_I,04_0003"><hi rend="capitalsize1">M</hi>en lachtert dat <num type="roman">.j.</num> hier seghet</l>
<lb n="2" xml:id="Mf2v.2"/><l n="M_I,04_0004"><hi rend="capitalsize1">D</hi>ies hi ghinder niet en pleghet</l>
</lg>
<lg type="disticha" n="I,05">
<lb n="3" xml:id="Mf2v.3"/><l n="M_I,05_0001"><hi rend="capitalsize1">N</hi>e mercke niemens quade daet</l>
<lb n="4" xml:id="Mf2v.4"/><l n="M_I,05_0002"><hi rend="capitalsize1">T</hi>e uele mercken dat es quaet</l>
<lb n="5" xml:id="Mf2v.5"/><l n="M_I,05_0003"><hi rend="capitalsize1">B</hi>edi en es man no wijf</l>
<lb n="6" xml:id="Mf2v.6"/><l n="M_I,05_0004"><hi rend="capitalsize1">D</hi>ie sonder sonde leet siin lijf</l>
</lg>
<lg type="disticha" n="I,08">
<lb n="7" xml:id="Mf2v.7"/><l n="M_I,08_0001"><hi rend="capitalsize1">G</hi>heloof niet als diin wijf <choice><abbr>to<g ref="#bar"/>rechte</abbr><expan>to<ex>n</ex>rechte</expan></choice></l>
<lb n="8" xml:id="Mf2v.8"/><l n="M_I,08_0002"><hi rend="capitalsize1">C</hi>laghet ouer hare knechte</l>
<lb n="9" xml:id="Mf2v.9"/><l n="M_I,08_0003"><hi rend="capitalsize1">D</hi>er wiuen siin ghenouch</l>
<lb n="10" xml:id="Mf2v.10"/><l n="M_I,08_0004"><hi rend="capitalsize1">T</hi>smans vrient siin tongheuouch</l>
</lg>
<lg type="disticha" n="I,10">
<lb n="11" xml:id="Mf2v.11"/><l n="M_I,10_0001"><hi rend="capitalsize1">D</hi>une sult met talen no spraken</l>
<lb n="12" xml:id="Mf2v.12"/><l n="M_I,10_0002"><hi rend="capitalsize1">G</hi>hescelt <choice><abbr>ieghe<g ref="#bar"/></abbr><expan>ieghe<ex>n</ex></expan></choice> den quade maken</l>
<lb n="13" xml:id="Mf2v.13"/><l n="M_I,10_0003"><hi rend="capitalsize1">E</hi>n sit niemens goeders wale</l>
<lb n="14" xml:id="Mf2v.14"/><l n="M_I,10_0004"><hi rend="capitalsize1">T</hi>ieghens hem te hebbene tale</l>
</lg>
<lg type="disticha" n="I,11">
<lb n="15" xml:id="Mf2v.15"/><l n="M_I,11_0001"><hi rend="capitalsize1">M</hi>inne die ghene die dii minnen</l>
<lb n="16" xml:id="Mf2v.16"/><l n="M_I,11_0002"><hi rend="capitalsize1">D</hi>ien du wel ions laet bekinnen</l>
<lb n="17" xml:id="Mf2v.17"/><l n="M_I,11_0003"><hi rend="capitalsize1">H</hi>i es dom die daer te dienste staet</l>
<lb n="18" xml:id="Mf2v.18"/><l n="M_I,11_0004"><hi rend="capitalsize1">D</hi>aert hem tscanden al vergaet</l>
</lg>
<lg type="disticha" n="I,12">
<lb n="19" xml:id="Mf2v.19"/><l n="M_I,12_0001"><hi rend="capitalsize1">N</hi>iemare ende achter spraken</l>
<lb n="20" xml:id="Mf2v.20"/><l n="M_I,12_0002"><hi rend="capitalsize1">D</hi>ie den lieden tscanden maken</l>


<pb xml:id="M.f3r" n="3r"/>
<lb n="1" xml:id="Mf3r.1"/><l n="M_I,12_0003"><hi rend="capitalsize1">S</hi>ultu scuwen ende haten</l>
<lb n="2" xml:id="Mf3r.2"/><l n="M_I,12_0004"><hi rend="capitalsize1">B</hi>eede in weghe <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> in straten</l>
</lg>
<lg type="disticha" n="I,13">
<lb n="3" xml:id="Mf3r.3"/><l n="M_I,13_0001"><hi rend="capitalsize1">D</hi>atmen di beheeten heeft</l>
<lb n="4" xml:id="Mf3r.4"/><l n="M_I,13_0002"><hi rend="capitalsize1">E</hi>er die tijt comt datmen di gheeft</l>
<lb n="5" xml:id="Mf3r.5"/><l n="M_I,13_0003"><hi rend="capitalsize1">N</hi>e sultuut niet beheeten voort</l>
<lb n="6" xml:id="Mf3r.6"/><l n="M_I,13_0004"><hi rend="capitalsize1">W</hi>ant men lieghet vele woort</l>
</lg>
<lg type="disticha" n="I,14">
<lb n="7" xml:id="Mf3r.7"/><l n="M_I,14_0001"><hi rend="capitalsize1">A</hi>lsti iement gheuet lof</l>
<lb n="8" xml:id="Mf3r.8"/><l n="M_I,14_0002"><hi rend="capitalsize1">N</hi>e wes te blider niet der of</l>
<lb n="9" xml:id="Mf3r.9"/><l n="M_I,14_0003"><hi rend="capitalsize1">W</hi>ant die vroede wel verstaet</l>
<lb n="10" xml:id="Mf3r.10"/><l n="M_I,14_0004"><hi rend="capitalsize1">E</hi>er hi van iement lof ontfaet</l>
</lg>
<lg type="disticha" n="I,15">
<lb n="11" xml:id="Mf3r.11"/><l n="M_I,15_0001"><hi rend="capitalsize1">A</hi>ls een ander man doet wel</l>
<lb n="12" xml:id="Mf3r.12"/><l n="M_I,15_0002"><hi rend="capitalsize1">D</hi>at onthout ende vertel</l>
<lb n="13" xml:id="Mf3r.13"/><l n="M_I,15_0003"><hi rend="capitalsize1">D</hi>oestu wel soe zwigher of</l>
<lb n="14" xml:id="Mf3r.14"/><l n="M_I,15_0004"><hi rend="capitalsize1">S</hi>oe sultu hebben goeden lof</l>
</lg>
<lg type="disticha" n="I,16">
<lb n="15" xml:id="Mf3r.15"/><l n="M_I,16_0001"><hi rend="capitalsize1">D</hi>je wile dattu best ionghelinc</l>
<lb n="16" xml:id="Mf3r.16"/><l n="M_I,16_0002"><hi rend="capitalsize1">D</hi>oet soe menighe goede dinc</l>
<lb n="17" xml:id="Mf3r.17"/><l n="M_I,16_0003"><hi rend="capitalsize1">A</hi>lsti naken dine daghen</l>
<lb n="18" xml:id="Mf3r.18"/><l n="M_I,16_0004"><choice><abbr><hi rend="capitalsize1">D</hi>atme<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">D</hi>atme<ex>n</ex></expan></choice> diins te goede <choice><abbr>ghewaghe<g ref="#bar"/></abbr><expan>ghewaghe<ex>n</ex></expan></choice></l>
</lg>
<lg type="disticha" n="I,18">
<lb n="19" xml:id="Mf3r.19"/><l n="M_I,18_0001"><hi rend="capitalsize1">S</hi>one alstu heues doghet</l>
<lb n="20" xml:id="Mf3r.20"/><l n="M_I,18_0002"><hi rend="capitalsize1">H</hi>auen houti int gheuouch</l>


<pb xml:id="M.f3v" n="3v"/>
<lb n="1" xml:id="Mf3v.1"/><l n="M_I,18_0003"><hi rend="capitalsize1">W</hi>ant heden es die man rike</l>
<lb n="2" xml:id="Mf3v.2"/><l n="M_I,18_0004"><hi rend="capitalsize1">S</hi>morghens staet hem aermelike</l>
</lg>
<lg type="disticha" n="I,20">
<lb n="3" xml:id="Mf3v.3"/><l n="M_I,20_0001"><hi rend="capitalsize1">S</hi>one als diin aerme vrient</l>
<lb n="4" xml:id="Mf3v.4"/><l n="M_I,20_0002"><hi rend="capitalsize1">V</hi>an eere cleender ghifte dient</l>
<lb n="5" xml:id="Mf3v.5"/><l n="M_I,20_0003"><hi rend="capitalsize1">W</hi>es blider dan di es te moede</l>
