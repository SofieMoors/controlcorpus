<lg type="disticha" n="III,03">
<lb n="3" xml:id="Dfavjv.3"/><l n="D_III,03_0001"><hi rend="capitalsize1">A</hi>lstu gheropen bist te rade</l>
<lb n="4" xml:id="Dfavjv.4"/><l n="D_III,03_0002"><hi rend="capitalsize1">S</hi>o en bedenct di niet te spade</l>
<lb n="5" xml:id="Dfavjv.5"/><l n="D_III,03_0003"><hi rend="capitalsize1">D</hi>u en beters dijn gesellen daet</l>
<lb n="6" xml:id="Dfavjv.6"/><l n="D_III,03_0004"><hi rend="capitalsize1">V</hi>an alle des dy ane gaet</l>
</lg>
<lg type="disticha" n="III,08">
<lb n="7" xml:id="Dfavjv.7"/><l n="D_III,08_0001"><hi rend="capitalsize1">D</hi>atti te deele beuallen es</l>
<lb n="8" xml:id="Dfavjv.8"/><l n="D_III,08_0002"><hi rend="capitalsize1">H</hi>out dat ende wachti des</l>
<lb n="9" xml:id="Dfavjv.9"/><l n="D_III,08_0003"><choice><abbr><hi rend="capitalsize1">D</hi>atme<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">D</hi>atme<ex>n</ex></expan></choice> niet en seit waer hij gaet</l>
<lb n="10" xml:id="Dfavjv.10"/><l n="D_III,08_0004"><hi rend="capitalsize1">D</hi>ie tsine verdede met quader daet</l>
</lg>
<lg type="disticha" n="III,10">
<lb n="11" xml:id="Dfavjv.11"/><l n="D_III,10_0001"><hi rend="capitalsize1">A</hi>ls di yemant gheeft raet</l>
<lb n="12" xml:id="Dfavjv.12"/><l n="D_III,10_0002"><hi rend="capitalsize1">H</hi>i en sal di niet <choice><abbr>duncke<g ref="#bar"/></abbr><expan>duncke<ex>n</ex></expan></choice> quaet</l>
<lb n="13" xml:id="Dfavjv.13"/><l n="D_III,10_0003"><hi rend="capitalsize1">H</hi>i is sot die raet ontseyt</l>
<lb n="14" xml:id="Dfavjv.14"/><l n="D_III,10_0004"><hi rend="capitalsize1">S</hi>o wien gaf daer doghet an leyt.</l>
</lg>
<lg type="disticha" n="III,14">
<lb n="15" xml:id="Dfavjv.15"/><l n="D_III,14_0001"><hi rend="capitalsize1">E</hi>n proeft niet dine cracht</l>
<lb n="16" xml:id="Dfavjv.16"/><l n="D_III,14_0002"><hi rend="capitalsize1">A</hi>en datti is tot uwer macht</l>
<lb n="17" xml:id="Dfavjv.17"/><l n="D_III,14_0003"><hi rend="capitalsize1">D</hi>aer is te prouen lachter an</l>
<lb n="18" xml:id="Dfavjv.18"/><l n="D_III,14_0004"><hi rend="capitalsize1">D</hi>at een ghedoen niet en can</l>
</lg>
<lg type="disticha" n="III,16">
<lb n="19" xml:id="Dfavjv.19"/><l n="D_III,16_0001"><hi rend="capitalsize1">A</hi>lstu verstaet in dinen moet</l>
<lb n="20" xml:id="Dfavjv.20"/><l n="D_III,16_0002"><hi rend="capitalsize1">D</hi>atmen di onrecht doet</l>


<pb xml:id="D.favijr" n="avijr"/>
<lb n="1" xml:id="Dfavijr.1"/><l n="D_III,16_0003"><hi rend="capitalsize1">B</hi>idt den rechter dat hi di</l>
<lb n="2" xml:id="Dfavijr.2"/><l n="D_III,16_0004"><hi rend="capitalsize1">O</hi>m dinen dienst behulpich si</l>
</lg>
<lg type="disticha" n="III,19">
<lb n="3" xml:id="Dfavijr.3"/><l n="D_III,19_0001"><hi rend="capitalsize1">A</hi>lstu etes wacht v dies</l>
<lb n="4" xml:id="Dfavijr.4"/><l n="D_III,19_0002"><hi rend="capitalsize1">D</hi>attu te vele niet en plies</l>
<lb n="5" xml:id="Dfavijr.5"/><l n="D_III,19_0003"><hi rend="capitalsize1">T</hi>e spreken wiet heeft in seden</l>
<lb n="6" xml:id="Dfavijr.6"/><l n="D_III,19_0004"><hi rend="capitalsize1">M</hi>en mercket al in <choice><abbr>dorperhede<g ref="#bar"/></abbr><expan>dorperhede<ex>n</ex></expan></choice></l>
</lg>
<lg type="disticha" n="IV,13">
<lb n="7" xml:id="Dfavijr.7"/><l n="D_IV,13_0001"><hi rend="capitalsize1">A</hi>ls di vernoy toe gaet</l>
<lb n="8" xml:id="Dfavijr.8"/><l n="D_IV,13_0002"><hi rend="capitalsize1">S</hi>oect an dine vrienden raet</l>
<lb n="9" xml:id="Dfavijr.9"/><l n="D_IV,13_0003"><hi rend="capitalsize1">D</hi>ie vrient die trouwe heeft</l>
<lb n="10" xml:id="Dfavijr.10"/><l n="D_IV,13_0004"><hi rend="capitalsize1">I</hi>s die beste troest die leeft</l>
</lg>
<lg type="disticha" n="IV,16">
<lb n="11" xml:id="Dfavijr.11"/><l n="D_IV,16_0001"><hi rend="capitalsize1">A</hi>ls du goet hebste <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> haue</l>
<lb n="12" xml:id="Dfavijr.12"/><l n="D_IV,16_0002"><hi rend="capitalsize1">N</hi>eemt v ghemack daer aue</l>
<lb n="13" xml:id="Dfavijr.13"/><l n="D_IV,16_0003"><hi rend="capitalsize1">W</hi>at helpt goet ende scat</l>
<lb n="14" xml:id="Dfavijr.14"/><l n="D_IV,16_0004"><hi rend="capitalsize1">D</hi>es nyemant en heeft te bat</l>
</lg>
<lg type="disticha" n="II,19">
<lb n="15" xml:id="Dfavijr.15"/><l n="D_II,19_0001"><hi rend="capitalsize1">H</hi>oerdom is een quade sede</l>
<lb n="16" xml:id="Dfavijr.16"/><l n="D_II,19_0002"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> vreck tsine/daer mede</l>
<lb n="17" xml:id="Dfavijr.17"/><l n="D_II,19_0003"><hi rend="capitalsize1">D</hi>ese twe soutstu scuwen</l>
<lb n="18" xml:id="Dfavijr.18"/><l n="D_II,19_0004"><choice><abbr><hi rend="capitalsize1">W</hi>a<g ref="#bar"/>t</abbr><expan><hi rend="capitalsize1">W</hi>a<ex>n</ex>t</expan></choice> si vele souden <choice><abbr>brouwe<g ref="#bar"/></abbr><expan>brouwe<ex>n</ex></expan></choice></l>
</lg>
<lg type="disticha" n="III,24">
<lb n="19" xml:id="Dfavijr.19"/><l n="D_III,24_0001"><hi rend="capitalsize1">D</hi>u sult met dinen sin algader</l>
<lb n="20" xml:id="Dfavijr.20"/><l n="D_III,24_0002"><hi rend="capitalsize1">E</hi>eren moeder ende vader.</l>


<pb xml:id="D.favijv" n="avijv"/>
<lb n="1" xml:id="Dfavijv.1"/><l n="D_III,24_0003"><hi rend="capitalsize1">G</hi>od seit diese belghen doet</l>
<lb n="2" xml:id="Dfavijv.2"/><l n="D_III,24_0004"><hi rend="capitalsize1">D</hi>at <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> te pine worden moet</l>
</lg>
<lg type="disticha" n="IV,24">
<lb n="3" xml:id="Dfavijv.3"/><l n="D_IV,24_0001"><hi rend="capitalsize1">T</hi>e vele eten <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> drincken</l>
<lb n="4" xml:id="Dfavijv.4"/><l n="D_IV,24_0002"><hi rend="capitalsize1">S</hi>outstu scuwen ende <choice><abbr>dencke<g ref="#bar"/></abbr><expan>dencke<ex>n</ex></expan></choice></l>
<lb n="5" xml:id="Dfavijv.5"/><l n="D_IV,24_0003"><hi rend="capitalsize1">W</hi>ie <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> hout an dit ghelaghe</l>
<lb n="6" xml:id="Dfavijv.6"/><l n="D_IV,24_0004"><hi rend="capitalsize1">H</hi>i corter mede sine daghe</l>
</lg>
<lg type="disticha" n="III,20">
<lb n="7" xml:id="Dfavijv.7"/><l n="D_III,20_0001"><hi rend="capitalsize1">A</hi>ls dijn wijf verbolghen es</l>
<lb n="8" xml:id="Dfavijv.8"/><l n="D_III,20_0002"><hi rend="capitalsize1">E</hi>n wilt niet roeken des</l>
<lb n="9" xml:id="Dfavijv.9"/><l n="D_III,20_0003"><hi rend="capitalsize1">W</hi>at si seyt want haer woert</l>
<lb n="10" xml:id="Dfavijv.10"/><l n="D_III,20_0004"><hi rend="capitalsize1">W</hi>aeyt die wint wter poert</l>
</lg>
<lg type="disticha" n="III,13">
<lb n="11" xml:id="Dfavijv.11"/><l n="D_III,13_0001"><hi rend="capitalsize1">D</hi>oet yemant wel wie hi sy</l>
<lb n="12" xml:id="Dfavijv.12"/><l n="D_III,13_0002"><hi rend="capitalsize1">N</hi>eemter goet exempel bi</l>
<lb n="13" xml:id="Dfavijv.13"/><l n="D_III,13_0003"><hi rend="capitalsize1">D</hi>oet <choice><abbr>yema<g ref="#bar"/>t</abbr><expan>yema<ex>n</ex>t</expan></choice> quaet saltu vlien</l>
<lb n="14" xml:id="Dfavijv.14"/><l n="D_III,13_0004"><hi rend="capitalsize1">E</hi>n laetne sijnre quaetheit plien</l>
</lg>
<lg type="disticha" n="II,26">
<lb n="15" xml:id="Dfavijv.15"/><l n="D_II,26_0001"><hi rend="capitalsize1">D</hi>ie heeft dat hem becoemt</l>
<lb n="16" xml:id="Dfavijv.16"/><l n="D_II,26_0002"><hi rend="capitalsize1">H</hi>out hi ic waent hem vroemt</l>
<lb n="17" xml:id="Dfavijv.17"/><l n="D_II,26_0003"><hi rend="capitalsize1">H</hi>i is sot die heeft goet</l>
<lb n="18" xml:id="Dfavijv.18"/><l n="D_II,26_0004"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> leghet onder sine voet</l>
</lg>
<lg type="disticha" n="II,18">
<lb n="19" xml:id="Dfavijv.19"/><l n="D_II,18_0001"><hi rend="capitalsize1">C</hi>onstu di gheueynsen vroet</l>
<lb n="20" xml:id="Dfavijv.20"/><l n="D_II,18_0002"><hi rend="capitalsize1">H</hi>et soudi dicke wesen goet</l>


<pb xml:id="D.faviiijr" n="aviiijr"/>
<lb n="1" xml:id="Dfaviiijr.1"/><l n="D_II,18_0003"><hi rend="capitalsize1">D</hi>ie vroede man winter an</l>
<lb n="2" xml:id="Dfaviiijr.2"/><l n="D_II,18_0004"><hi rend="capitalsize1">D</hi>icke dat hi hem veysen can</l>
</lg>
<lg type="disticha" n="I,27">
<lb n="3" xml:id="Dfaviiijr.3"/><l n="D_I,27_0001"><hi rend="capitalsize1">W</hi>achti ieghen die ghebare</l>
<lb n="4" xml:id="Dfaviiijr.4"/><l n="D_I,27_0002"><hi rend="capitalsize1">M</hi>inlick als die voghelare</l>
<lb n="5" xml:id="Dfaviiijr.5"/><l n="D_I,27_0003"><hi rend="capitalsize1">D</hi>ie scone pipen <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> blasen</l>
<lb n="6" xml:id="Dfaviiijr.6"/><l n="D_I,27_0004"><hi rend="capitalsize1">A</hi>ls si die vogelkijns <choice><abbr>v<g ref="#apomod"/>dwasen</abbr><expan>v<ex>er</ex>dwasen</expan></choice></l>
</lg>
<lg type="disticha" n="III,12">
<lb n="7" xml:id="Dfaviiijr.7"/><l n="D_III,12_0001"><hi rend="capitalsize1">W</hi>achti als bi dinen liue</l>
<lb n="8" xml:id="Dfaviiijr.8"/><l n="D_III,12_0002"><hi rend="capitalsize1">D</hi>attu gheen wijf te wiue</l>
<lb n="9" xml:id="Dfaviiijr.9"/><l n="D_III,12_0003"><hi rend="capitalsize1">E</hi>n nemes du en kentse wel</l>
<lb n="10" xml:id="Dfaviiijr.10"/><l n="D_III,12_0004"><choice><abbr><hi rend="capitalsize1">W</hi>a<g ref="#bar"/>t</abbr><expan><hi rend="capitalsize1">W</hi>a<ex>n</ex>t</expan></choice> der wiuen sijn vele fel</l>
</lg>
<lg type="disticha" n="independent stanza">
<lb n="11" xml:id="Dfaviiijr.11"/><l n="D_independent stanza_0001"><hi rend="capitalsize1">S</hi>one dits den besten scat</l>
<lb n="12" xml:id="Dfaviiijr.12"/><l n="D_independent stanza_0002"><hi rend="capitalsize1">D</hi>ie ick hebbe ende om dat.</l>
<lb n="13" xml:id="Dfaviiijr.13"/><l n="D_independent stanza_0003"><hi rend="capitalsize1">H</hi>eet ic ende beuele di</l>
<lb n="14" xml:id="Dfaviiijr.14"/><l n="D_independent stanza_0004"><hi rend="capitalsize1">D</hi>atti altoes met di si</l>
</lg>
</p>
</body>
</text>


<text n="epilogue" xml:id="epilogue">
<body>
<p><lg type="disticha" n="epilogue">
<lb n="15" xml:id="Dfaviiijr.15"/><l n="D_epilogue_0001"><hi rend="capitalsize1">G</hi>oede lieden die dese tale</l>
<lb n="16" xml:id="Dfaviiijr.16"/><l n="D_epilogue_0002"><hi rend="capitalsize1">H</hi>ebt ghehoort alte male</l>
<lb n="17" xml:id="Dfaviiijr.17"/><l n="D_epilogue_0003"><hi rend="capitalsize1">D</hi>oet v ghebede deuotelike</l>
<lb n="18" xml:id="Dfaviiijr.18"/><l n="D_epilogue_0004"><hi rend="capitalsize1">T</hi>ot gode van hemelrike</l>
<lb n="19" xml:id="Dfaviiijr.19"/><l n="D_epilogue_0005"><hi rend="capitalsize1">D</hi>at hi verlichte uwen sin</l>
<lb n="20" xml:id="Dfaviiijr.20"/><l n="D_epilogue_0006"><hi rend="capitalsize1">D</hi>atter wone wijsheit in</l>


<pb xml:id="D.faviijv" n="aviijv"/>
<lb n="1" xml:id="Dfaviijv.1"/><l n="D_epilogue_0007"><hi rend="capitalsize1">D</hi>ie dit maecte vten latine</l>
<lb n="2" xml:id="Dfaviijv.2"/><l n="D_epilogue_0008"><hi rend="capitalsize1">I</hi>n duytsche hi begheerde te sine</l>
<lb n="3" xml:id="Dfaviijv.3"/><l n="D_epilogue_0009"><hi rend="capitalsize1">I</hi>n v ghebede vroe <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> spade</l>
<lb n="4" xml:id="Dfaviijv.4"/><l n="D_epilogue_0010"><hi rend="capitalsize1">B</hi>idt gode dat hi v doe ghenade</l>

