    <text xml:id="DietscheCatoen">
        <group><pb xml:id="D.fajv" n="ajv"/>


<text n="prologue" xml:id="prologue">
<body>
<p><lg type="disticha" n="title">
<lb n="1" xml:id="Dfajv.1"/><l n="D_title_0001">¶ <hi rend="capitalsize1">D</hi>en duytschen <hi rend="capitalsize1">C</hi>athoen.</l>
</lg>
<lg type="disticha" n="prologue">
<lb n="2" xml:id="Dfajv.2"/><l n="D_prologue_0001"><hi rend="capitalsize2">D</hi>Ie ghene die in <choice><abbr>hare<g ref="#bar"/></abbr><expan>hare<ex>n</ex></expan></choice> sinne</l>
<lb n="3" xml:id="Dfajv.3"/><l n="D_prologue_0002"><choice><abbr><hi rend="capitalsize1">D</hi>raghe<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">D</hi>raghe<ex>n</ex></expan></choice> waerlike minne</l>
<lb n="4" xml:id="Dfajv.4"/><l n="D_prologue_0003"><hi rend="capitalsize1">S</hi>i maken daer of rijm ende liet</l>
<lb n="5" xml:id="Dfajv.5"/><l n="D_prologue_0004"><hi rend="capitalsize1">D</hi>er <choice><abbr>minne<g ref="#bar"/></abbr><expan>minne<ex>n</ex></expan></choice> so en draghe ic niet</l>
<lb n="6" xml:id="Dfajv.6"/><l n="D_prologue_0005"><hi rend="capitalsize1">W</hi>ant mi es al dat verghaen</l>
<lb n="7" xml:id="Dfajv.7"/><l n="D_prologue_0006"><hi rend="capitalsize1">D</hi>atter <choice><abbr>minne<g ref="#bar"/></abbr><expan>minne<ex>n</ex></expan></choice> mach bestaen</l>
<lb n="8" xml:id="Dfajv.8"/><l n="D_prologue_0007"><hi rend="capitalsize1">T</hi>er wilen doen ic minne droech</l>
<lb n="9" xml:id="Dfajv.9"/><l n="D_prologue_0008"><hi rend="capitalsize1">A</hi>ls ic die minne sach ick loech</l>
<lb n="10" xml:id="Dfajv.10"/><l n="D_prologue_0009"><hi rend="capitalsize1">N</hi>v haet icse al in minen <choice><abbr>sinne<g ref="#bar"/></abbr><expan>sinne<ex>n</ex></expan></choice></l>
<lb n="11" xml:id="Dfajv.11"/><l n="D_prologue_0010"><hi rend="capitalsize1">D</hi>ie minne <choice><abbr>draghe<g ref="#bar"/></abbr><expan>draghe<ex>n</ex></expan></choice> <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> <choice><abbr>minne<g ref="#bar"/></abbr><expan>minne<ex>n</ex></expan></choice></l>
<lb n="12" xml:id="Dfajv.12"/><l n="D_prologue_0011"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> heb gheleert in minen moet</l>
<lb n="13" xml:id="Dfajv.13"/><l n="D_prologue_0012"><hi rend="capitalsize1">A</hi>en die ghene die sijn vroet</l>
<lb n="14" xml:id="Dfajv.14"/><l n="D_prologue_0013"><hi rend="capitalsize1">E</hi>er ict dede seide een <choice><abbr>mij<g ref="#bar"/></abbr><expan>mij<ex>n</ex></expan></choice> vrient</l>
<lb n="15" xml:id="Dfajv.15"/><l n="D_prologue_0014"><hi rend="capitalsize1">G</hi>eselle die ter tijt wijsheit dient.</l>
<lb n="16" xml:id="Dfajv.16"/><l n="D_prologue_0015"><hi rend="capitalsize1">D</hi>at hijs beteren loen ontfaet</l>
<lb n="17" xml:id="Dfajv.17"/><l n="D_prologue_0016"><hi rend="capitalsize1">D</hi>an die ter <choice><abbr>minne<g ref="#bar"/></abbr><expan>minne<ex>n</ex></expan></choice> dienste staet</l>
<lb n="18" xml:id="Dfajv.18"/><l n="D_prologue_0017"><hi rend="capitalsize1">H</hi>ier om heb ic des begonnen</l>
<lb n="19" xml:id="Dfajv.19"/><l n="D_prologue_0018"><hi rend="capitalsize1">S</hi>i <choice><abbr>hebbe<g ref="#bar"/></abbr><expan>hebbe<ex>n</ex></expan></choice> onrecht des mi <choice><abbr>v<g ref="#apomod"/>gonnen</abbr><expan>v<ex>er</ex>gonnen</expan></choice></l>
<lb n="20" xml:id="Dfajv.20"/><l n="D_prologue_0019"><hi rend="capitalsize1">N</hi>v bidt alle met mi te gode</l>
<lb n="21" xml:id="Dfajv.21"/><l n="D_prologue_0020"><hi rend="capitalsize1">D</hi>at hi mi sende sinen bode</l>


<pb xml:id="D.faijr" n="aijr"/>
<lb n="1" xml:id="Dfaijr.1"/><l n="D_prologue_0021"><hi rend="capitalsize1">D</hi>ie mij wise ende leere</l>
<lb n="2" xml:id="Dfaijr.2"/><l n="D_prologue_0022"><hi rend="capitalsize1">H</hi>oe ic mi ter wijsheit keere</l>
<lb n="3" xml:id="Dfaijr.3"/><l n="D_prologue_0023"><hi rend="capitalsize1">A</hi>lle die vroet willen wesen</l>
<lb n="4" xml:id="Dfaijr.4"/><l n="D_prologue_0024"><hi rend="capitalsize1">E</hi>en boeck is dat die <choice><abbr>clercke<g ref="#bar"/></abbr><expan>clercke<ex>n</ex></expan></choice> lesen</l>
<lb n="5" xml:id="Dfaijr.5"/><l n="D_prologue_0025"><hi rend="capitalsize1">A</hi>ls si eerst ter scolen gaen</l>
<lb n="6" xml:id="Dfaijr.6"/><l n="D_prologue_0026"><hi rend="capitalsize1">D</hi>ie <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> wijsheyt doet verstaen</l>
<lb n="7" xml:id="Dfaijr.7"/><l n="D_prologue_0027"><hi rend="capitalsize1">V</hi>ele meer dan eenich doet</l>
<lb n="8" xml:id="Dfaijr.8"/><l n="D_prologue_0028"><hi rend="capitalsize1">H</hi>ier om peynse ic in <choice><abbr>mine<g ref="#bar"/></abbr><expan>mine<ex>n</ex></expan></choice> moet</l>
<lb n="9" xml:id="Dfaijr.9"/><l n="D_prologue_0029"><hi rend="capitalsize1">D</hi>at icker v bi wille leren</l>
<lb n="10" xml:id="Dfaijr.10"/><l n="D_prologue_0030"><hi rend="capitalsize1">H</hi>oe ghi v herte sult <choice><abbr>bekere<g ref="#bar"/></abbr><expan>bekere<ex>n</ex></expan></choice></l>
<lb n="11" xml:id="Dfaijr.11"/><l n="D_prologue_0031"><hi rend="capitalsize1">T</hi>er vroetheit nv merct dan</l>
<lb n="12" xml:id="Dfaijr.12"/><l n="D_prologue_0032"><hi rend="capitalsize1">W</hi>ien maecte het <choice><abbr>woe<g ref="#bar"/>de</abbr><expan>woe<ex>n</ex>de</expan></choice> een man</l>
<lb n="13" xml:id="Dfaijr.13"/><l n="D_prologue_0033"><hi rend="capitalsize1">T</hi>e romen wijlen eer</l>
<lb n="14" xml:id="Dfaijr.14"/><l n="D_prologue_0034"><hi rend="capitalsize1">D</hi>ie der wijsheit wiste meer.</l>
<lb n="15" xml:id="Dfaijr.15"/><l n="D_prologue_0035"><hi rend="capitalsize1">D</hi>an yemant die leeft nv</l>
<lb n="16" xml:id="Dfaijr.16"/><l n="D_prologue_0036"><hi rend="capitalsize1">H</hi>i hiet <hi rend="capitalsize1">C</hi>athoen dat seg ic v</l>
<lb n="17" xml:id="Dfaijr.17"/><l n="D_prologue_0037"><hi rend="capitalsize1">D</hi>ie hadde een kint dat hiet also</l>
<lb n="18" xml:id="Dfaijr.18"/><l n="D_prologue_0038"><hi rend="capitalsize1">A</hi>l dat hi dede des was hy vro</l>
<lb n="19" xml:id="Dfaijr.19"/><l n="D_prologue_0039"><hi rend="capitalsize1">A</hi>lst metten kinde was vergaen</l>
<lb n="20" xml:id="Dfaijr.20"/><l n="D_prologue_0040"><hi rend="capitalsize1">D</hi>at mocht onthouden saen.</l>


<pb xml:id="D.faijv" n="aijv"/>
<lb n="1" xml:id="Dfaijv.1"/><l n="D_prologue_0041"><hi rend="capitalsize1">R</hi>iep hi ende sprac tot hem</l>
<lb n="2" xml:id="Dfaijv.2"/><l n="D_prologue_0042"><hi rend="capitalsize1">M</hi>en seyt sone dat ic ben</l>
<lb n="3" xml:id="Dfaijv.3"/><l n="D_prologue_0043"><hi rend="capitalsize1">W</hi>ijs vroet nv merct dan</l>
<lb n="4" xml:id="Dfaijv.4"/><l n="D_prologue_0044"><hi rend="capitalsize1">I</hi>ck sal v leren dat ic kan</l>
<lb n="5" xml:id="Dfaijv.5"/><l n="D_prologue_0045"><hi rend="capitalsize1">D</hi>oen seide hi <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> begonst aldus</l>
<lb n="6" xml:id="Dfaijv.6"/><l n="D_prologue_0046"><hi rend="capitalsize1">S</hi>i deus est animus.</l>
</lg>
</p>
</body>
</text>


<text n="disticha" xml:id="disticha">
<body>
<p><lg type="disticha" n="I,01">
<lb n="7" xml:id="Dfaijv.7"/><l n="D_I,01_0001"><hi rend="capitalsize1">N</hi>v merct sone wat ic v ghebiede</l>
<lb n="8" xml:id="Dfaijv.8"/><l n="D_I,01_0002"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> wat is dat ic v bediede</l>
<lb n="9" xml:id="Dfaijv.9"/><l n="D_I,01_0003"><hi rend="capitalsize1">D</hi>attu di daer aen sout keren</l>
<lb n="10" xml:id="Dfaijv.10"/><l n="D_I,01_0004"><hi rend="capitalsize1">D</hi>attu gode sout emmer eren</l>
</lg>
<lg type="disticha" n="I,02">
<lb n="11" xml:id="Dfaijv.11"/><l n="D_I,02_0001"><hi rend="capitalsize1">D</hi>u soutst <choice><abbr>smorge<g ref="#bar"/>s</abbr><expan>smorge<ex>n</ex>s</expan></choice> vroe opstaen</l>
<lb n="12" xml:id="Dfaijv.12"/><l n="D_I,02_0002"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> om dine bederue gaen</l>
<lb n="13" xml:id="Dfaijv.13"/><l n="D_I,02_0003"><hi rend="capitalsize1">M</hi>en seit die te langhe slaept</l>
<lb n="14" xml:id="Dfaijv.14"/><l n="D_I,02_0004"><hi rend="capitalsize1">D</hi>at <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> die slaep onduchtich maect</l>
</lg>
<lg type="disticha" n="I,03">
<lb n="15" xml:id="Dfaijv.15"/><l n="D_I,03_0001"><hi rend="capitalsize1">W</hi>achti datmen niet en seyt</l>
<lb n="16" xml:id="Dfaijv.16"/><l n="D_I,03_0002"><hi rend="capitalsize1">D</hi>at vele talen aen di leyt</l>
<lb n="17" xml:id="Dfaijv.17"/><l n="D_I,03_0003"><hi rend="capitalsize1">W</hi>ant <choice><abbr>menigerha<g ref="#bar"/>de</abbr><expan>menigerha<ex>n</ex>de</expan></choice> tale</l>
<lb n="18" xml:id="Dfaijv.18"/><l n="D_I,03_0004"><hi rend="capitalsize1">V</hi>oeghet <choice><abbr>nyema<g ref="#bar"/>t</abbr><expan>nyema<ex>n</ex>t</expan></choice> goeders wale</l>
</lg>
<lg type="disticha" n="I,04">
<lb n="19" xml:id="Dfaijv.19"/><l n="D_I,04_0001"><hi rend="capitalsize1">S</hi>one doet als die vroede doet</l>
<lb n="20" xml:id="Dfaijv.20"/><l n="D_I,04_0002"><hi rend="capitalsize1">D</hi>ie heeft gestadeliken moet</l>


<pb xml:id="D.faiijr" n="aiijr"/>
<lb n="1" xml:id="Dfaiijr.1"/><l n="D_I,04_0003"><choice><abbr><hi rend="capitalsize1">M</hi>e<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">M</hi>e<ex>n</ex></expan></choice> lachtert dat een hier segget</l>
<lb n="2" xml:id="Dfaiijr.2"/><l n="D_I,04_0004"><hi rend="capitalsize1">D</hi>es hi ghinder niet en pleghet</l>
</lg>
<lg type="disticha" n="I,07">
<lb n="3" xml:id="Dfaiijr.3"/><l n="D_I,07_0001"><hi rend="capitalsize1">E</hi>enradich ende sachte</l>
<lb n="4" xml:id="Dfaiijr.4"/><l n="D_I,07_0002"><hi rend="capitalsize1">S</hi>outs sijn in dijn gedachte.</l>
<lb n="5" xml:id="Dfaiijr.5"/><l n="D_I,07_0003"><hi rend="capitalsize1">H</hi>i is sot die dijn ghepeyns</l>
<lb n="6" xml:id="Dfaiijr.6"/><l n="D_I,07_0004"><hi rend="capitalsize1">D</hi>ie lieden seit hier <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> ghens</l>
</lg>
<lg type="disticha" n="I,05">
<lb n="7" xml:id="Dfaiijr.7"/><l n="D_I,05_0001"><hi rend="capitalsize1">M</hi>ercke <choice><abbr>nyema<g ref="#bar"/>ts</abbr><expan>nyema<ex>n</ex>ts</expan></choice> quade daet</l>
<lb n="8" xml:id="Dfaiijr.8"/><l n="D_I,05_0002"><choice><abbr><hi rend="capitalsize1">W</hi>a<g ref="#bar"/>t</abbr><expan><hi rend="capitalsize1">W</hi>a<ex>n</ex>t</expan></choice> vele <choice><abbr>mercke<g ref="#bar"/>s</abbr><expan>mercke<ex>n</ex>s</expan></choice> is quaet</l>
<lb n="9" xml:id="Dfaiijr.9"/><l n="D_I,05_0003"><hi rend="capitalsize1">B</hi>eyde en is man noch wijf</l>
<lb n="10" xml:id="Dfaiijr.10"/><l n="D_I,05_0004"><hi rend="capitalsize1">D</hi>ie sonder sonde leit sijn lijf.</l>
</lg>
<lg type="disticha" n="I,06">
<lb n="11" xml:id="Dfaiijr.11"/><l n="D_I,06_0001"><hi rend="capitalsize1">H</hi>outu oec dat yemant deert</l>
<lb n="12" xml:id="Dfaiijr.12"/><l n="D_I,06_0002"><hi rend="capitalsize1">D</hi>oet <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> di hi is sot dies begeert</l>
<lb n="13" xml:id="Dfaiijr.13"/><l n="D_I,06_0003"><hi rend="capitalsize1">O</hi>m dat luttel helpen mach</l>
<lb n="14" xml:id="Dfaiijr.14"/><l n="D_I,06_0004"><hi rend="capitalsize1">T</hi>e doen den lieden quaet ghelach.</l>
</lg>
<lg type="disticha" n="I,08">
<lb n="15" xml:id="Dfaiijr.15"/><l n="D_I,08_0001"><hi rend="capitalsize1">G</hi>heloeft niet dijn wijf tonrechte</l>
<lb n="16" xml:id="Dfaiijr.16"/><l n="D_I,08_0002"><hi rend="capitalsize1">A</hi>ls si claget ouer dine knechte</l>
<lb n="17" xml:id="Dfaiijr.17"/><l n="D_I,08_0003"><hi rend="capitalsize1">Q</hi>uader wijuen sijn genoech</l>
<lb n="18" xml:id="Dfaiijr.18"/><l n="D_I,08_0004"><hi rend="capitalsize1">D</hi>ie dicke maken ongeuoech.</l>
</lg>
<lg type="disticha" n="I,10">
<lb n="19" xml:id="Dfaiijr.19"/><l n="D_I,10_0001"><hi rend="capitalsize1">D</hi>u en selt niet talen noch <choice><abbr>spreke<g ref="#bar"/></abbr><expan>spreke<ex>n</ex></expan></choice></l>
<lb n="20" xml:id="Dfaiijr.20"/><l n="D_I,10_0002"><hi rend="capitalsize1">G</hi>heschel <choice><abbr>teghe<g ref="#bar"/></abbr><expan>teghe<ex>n</ex></expan></choice> die quade <choice><abbr>make<g ref="#bar"/></abbr><expan>make<ex>n</ex></expan></choice></l>


<pb xml:id="D.faiijv" n="aiijv"/>
<lb n="1" xml:id="Dfaiijv.1"/><l n="D_I,10_0003"><hi rend="capitalsize1">W</hi>ant van lichteliker chosen</l>
<lb n="2" xml:id="Dfaiijv.2"/><l n="D_I,10_0004"><hi rend="capitalsize1">C</hi>omen dicke grote nosen</l>
</lg>
<lg type="disticha" n="I,11">
<lb n="3" xml:id="Dfaiijv.3"/><l n="D_I,11_0001"><hi rend="capitalsize1">M</hi>inne <choice><abbr>de<g ref="#bar"/></abbr><expan>de<ex>n</ex></expan></choice> ghene die di <choice><abbr>minne<g ref="#bar"/></abbr><expan>minne<ex>n</ex></expan></choice></l>
<lb n="4" xml:id="Dfaiijv.4"/><l n="D_I,11_0002"><hi rend="capitalsize1">D</hi>ie di wel doet laet <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> <choice><abbr>bekenne<g ref="#bar"/></abbr><expan>bekenne<ex>n</ex></expan></choice></l>
<lb n="5" xml:id="Dfaiijv.5"/><l n="D_I,11_0003"><hi rend="capitalsize1">H</hi>i is sot die te dienste staet</l>
<lb n="6" xml:id="Dfaiijv.6"/><l n="D_I,11_0004"><hi rend="capitalsize1">D</hi>aert <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> tscanden al vergaet.</l>
</lg>
<lg type="disticha" n="I,12">
<lb n="7" xml:id="Dfaiijv.7"/><l n="D_I,12_0001"><hi rend="capitalsize1">N</hi>yemare <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> achterspraken</l>
<lb n="8" xml:id="Dfaiijv.8"/><l n="D_I,12_0002"><hi rend="capitalsize1">D</hi>ie de lieden tscanden maken.</l>
<lb n="9" xml:id="Dfaiijv.9"/><l n="D_I,12_0003"><hi rend="capitalsize1">S</hi>oudu scuwen ende haten</l>
<lb n="10" xml:id="Dfaiijv.10"/><l n="D_I,12_0004"><hi rend="capitalsize1">D</hi>ie haer <choice><abbr>volge<g ref="#bar"/></abbr><expan>volge<ex>n</ex></expan></choice> sijn verwaten</l>
</lg>
<lg type="disticha" n="I,13">
<lb n="11" xml:id="Dfaiijv.11"/><l n="D_I,13_0001"><hi rend="capitalsize1">D</hi>atmen di beheten heeft</l>
