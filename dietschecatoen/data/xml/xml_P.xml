<text n="disticha" xml:id="disticha">
<body>
<p><lg type="disticha" n="I,01">
<lb n="1" xml:id="Pf8va.1"/><l n="P_I,01_0001"><hi rend="capitalsize1">N</hi>u merct sone wattic hu ghebiede</l>
<lb n="2" xml:id="Pf8va.2"/><l n="P_I,01_0002"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> wat is dattic dy bediede</l>
<lb n="3" xml:id="Pf8va.3"/><l n="P_I,01_0003"><hi rend="capitalsize1">D</hi>ats dattu <choice><abbr>d<g ref="#apomod"/></abbr><expan>d<ex>aer</ex></expan></choice> ane sult <choice><abbr>keere<g ref="#bar"/></abbr><expan>keere<ex>n</ex></expan></choice></l>
<lb n="4" xml:id="Pf8va.4"/><l n="P_I,01_0004"><hi rend="capitalsize1">D</hi>attu gode zult emmer <choice><abbr>eere<g ref="#bar"/></abbr><expan>eere<ex>n</ex></expan></choice></l>
</lg>
<lg type="disticha" n="I,02">
<lb n="5" xml:id="Pf8va.5"/><l n="P_I,02_0001"><hi rend="capitalsize1">D</hi>u zult <choice><abbr>smorghe<g ref="#bar"/>s</abbr><expan>smorghe<ex>n</ex>s</expan></choice> vrouch vp <choice><abbr>stae<g ref="#bar"/></abbr><expan>stae<ex>n</ex></expan></choice></l>
<lb n="6" xml:id="Pf8va.6"/><l n="P_I,02_0002"><hi rend="capitalsize1">E</hi>nde om dine bederue gaen</l>


<pb xml:id="P.f8vb" n="8vb"/>
<lb n="1" xml:id="Pf8vb.1"/><l n="P_I,02_0003"><hi rend="capitalsize1">M</hi>en seit die te langhe slaept</l>
<lb n="2" xml:id="Pf8vb.2"/><l n="P_I,02_0004"><hi rend="capitalsize1">D</hi>at hem die slaep <choice><abbr>o<g ref="#bar"/>duchtic</abbr><expan>o<ex>n</ex>duchtic</expan></choice> mae<unclear>ct</unclear></l>
</lg>
<lg type="disticha" n="I,03">
<lb n="3" xml:id="Pf8vb.3"/><l n="P_I,03_0001"><hi rend="capitalsize1">W</hi>achti dat <choice><abbr>me<g ref="#bar"/></abbr><expan>me<ex>n</ex></expan></choice> niet en zeghet</l>
<lb n="4" xml:id="Pf8vb.4"/><l n="P_I,03_0002"><hi rend="capitalsize1">D</hi>at vele tale an dij leghet</l>
<lb n="5" xml:id="Pf8vb.5"/><l n="P_I,03_0003"><choice><abbr><hi rend="capitalsize1">W</hi>a<g ref="#bar"/>t</abbr><expan><hi rend="capitalsize1">W</hi>a<ex>n</ex>t</expan></choice> menegherande tale</l>
<lb n="6" xml:id="Pf8vb.6"/><l n="P_I,03_0004"><hi rend="capitalsize1">E</hi>n voughet <choice><abbr>nieme<g ref="#bar"/>t</abbr><expan>nieme<ex>n</ex>t</expan></choice> goeders wale</l>


<pb xml:id="P.f9va" n="9va"/>
</lg>
<lg type="disticha" n="I,04">
<lb n="1" xml:id="Pf9va.1"/><l n="P_I,04_0001"><hi rend="capitalsize1">Z</hi>one doet als die vroede doet</l>
<lb n="2" xml:id="Pf9va.2"/><l n="P_I,04_0002"><hi rend="capitalsize1">D</hi>ie heift ghestadeghen moet</l>
<lb n="3" xml:id="Pf9va.3"/><l n="P_I,04_0003"><hi rend="capitalsize1">H</hi>ets lachter dat <choice><abbr>me<g ref="#bar"/></abbr><expan>me<ex>n</ex></expan></choice> hier zeghet</l>
<lb n="4" xml:id="Pf9va.4"/><l n="P_I,04_0004"><hi rend="capitalsize1">D</hi>ies <choice><abbr>me<g ref="#bar"/></abbr><expan>me<ex>n</ex></expan></choice> ginder niet en pleghet</l>
</lg>
<lg type="disticha" n="I,05">
<lb n="5" xml:id="Pf9va.5"/><l n="P_I,05_0001"><hi rend="capitalsize1">E</hi>n merke <choice><abbr>nieme<g ref="#bar"/>s</abbr><expan>nieme<ex>n</ex>s</expan></choice> quade daet</l>
<lb n="6" xml:id="Pf9va.6"/><l n="P_I,05_0002"><choice><abbr><hi rend="capitalsize1">W</hi>a<g ref="#bar"/>t</abbr><expan><hi rend="capitalsize1">W</hi>a<ex>n</ex>t</expan></choice> vele merkens is quaet</l>
<lb n="7" xml:id="Pf9va.7"/><l n="P_I,05_0003"><hi rend="capitalsize1">B</hi>eede en es man noch wijf</l>
<lb n="8" xml:id="Pf9va.8"/><l n="P_I,05_0004"><hi rend="capitalsize1">D</hi>ie zonder zonde leet <choice><abbr>zij<g ref="#bar"/></abbr><expan>zij<ex>n</ex></expan></choice> lijf</l>
</lg>
<lg type="disticha" n="I,14">
<lb n="9" xml:id="Pf9va.9"/><l n="P_I,14_0001"><hi rend="capitalsize1">A</hi>ls di yement gheue lof</l>
<lb n="10" xml:id="Pf9va.10"/><l n="P_I,14_0002"><hi rend="capitalsize1">E</hi>n weist te blijder niet daer of</l>
<lb n="11" xml:id="Pf9va.11"/><l n="P_I,14_0003"><hi rend="capitalsize1">D</hi>ie vroede mercket <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> <choice><abbr>v<g ref="#apomod"/>staet</abbr><expan>v<ex>er</ex>staet</expan></choice></l>
<lb n="12" xml:id="Pf9va.12"/><l n="P_I,14_0004"><hi rend="capitalsize1">E</hi>er hij van yement lof <choice><abbr>o<g ref="#bar"/>tfaet</abbr><expan>o<ex>n</ex>tfaet</expan></choice></l>


<pb xml:id="P.f10ra" n="10ra"/>
</lg>
<lg type="disticha" n="I,16">
<lb n="1" xml:id="Pf10ra.1"/><l n="P_I,16_0001"><hi rend="capitalsize1">D</hi>ie wile du best jonghelinc</l>
<lb n="2" xml:id="Pf10ra.2"/><l n="P_I,16_0002"><hi rend="capitalsize1">Z</hi>o doet meneghe goede dinc</l>
<lb n="3" xml:id="Pf10ra.3"/><l n="P_I,16_0003"><hi rend="capitalsize1">A</hi>ls di naken dine daghe</l>
<lb n="4" xml:id="Pf10ra.4"/><l n="P_I,16_0004"><hi rend="capitalsize1">D</hi>at <choice><abbr>me<g ref="#bar"/></abbr><expan>me<ex>n</ex></expan></choice> dijns te goede ghewaghe</l>
</lg>
<lg type="disticha" n="I,18">
<lb n="5" xml:id="Pf10ra.5"/><l n="P_I,18_0001"><hi rend="capitalsize1">A</hi>l eist dat<add>s</add>tu hebbes ghenouch</l>
<lb n="6" xml:id="Pf10ra.6"/><l n="P_I,18_0002"><hi rend="capitalsize1">H</hi>aue houtti jnt gheuouch</l>
<lb n="7" xml:id="Pf10ra.7"/><l n="P_I,18_0003"><choice><abbr><hi rend="capitalsize1">W</hi>a<g ref="#bar"/>t</abbr><expan><hi rend="capitalsize1">W</hi>a<ex>n</ex>t</expan></choice> heden is de man rijcke</l>
<lb n="8" xml:id="Pf10ra.8"/><l n="P_I,18_0004"><hi rend="capitalsize1">Z</hi>morghens staet <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> <choice><abbr>co<g ref="#bar"/>merlike</abbr><expan>co<ex>m</ex>merlike</expan></choice></l>
</lg>
<lg type="disticha" n="I,20">
<lb n="9" xml:id="Pf10ra.9"/><l n="P_I,20_0001"><hi rend="capitalsize1">Z</hi>one als di <choice><abbr>dij<g ref="#bar"/></abbr><expan>dij<ex>n</ex></expan></choice> aerme vrient</l>
<lb n="10" xml:id="Pf10ra.10"/><l n="P_I,20_0002"><hi rend="capitalsize1">V</hi>an cleender ghiften dient</l>
<lb n="11" xml:id="Pf10ra.11"/><l n="P_I,20_0003"><hi rend="capitalsize1">W</hi>eist blider <choice><abbr>da<g ref="#bar"/></abbr><expan>da<ex>n</ex></expan></choice> di is te moede</l>
<lb n="12" xml:id="Pf10ra.12"/><l n="P_I,20_0004"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> dancket <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> met allen goede</l>
</lg>
<lg type="disticha" n="I,21">
<lb n="13" xml:id="Pf10ra.13"/><l n="P_I,21_0001"><hi rend="capitalsize1">A</hi>l bestu aerm <choice><abbr>ma<g ref="#bar"/></abbr><expan>ma<ex>n</ex></expan></choice> van hauen</l>
<lb n="14" xml:id="Pf10ra.14"/><l n="P_I,21_0002"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> van rijcheden bescauen</l>
<lb n="15" xml:id="Pf10ra.15"/><l n="P_I,21_0003"><hi rend="capitalsize1">B</hi>epeinst dattu hier te <choice><abbr>voore<g ref="#bar"/></abbr><expan>voore<ex>n</ex></expan></choice></l>
<lb n="16" xml:id="Pf10ra.16"/><l n="P_I,21_0004"><hi rend="capitalsize1">A</hi>erm <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> naect waert <choice><abbr>ghebore<g ref="#bar"/></abbr><expan>ghebore<ex>n</ex></expan></choice></l>
</lg>
<lg type="disticha" n="I,22">
<lb n="17" xml:id="Pf10ra.17"/><l n="P_I,22_0001"><hi rend="capitalsize1">D</hi>u ne zulstu <choice><abbr>keere<g ref="#bar"/></abbr><expan>keere<ex>n</ex></expan></choice> niet jn dien</l>
<lb n="18" xml:id="Pf10ra.18"/><l n="P_I,22_0002"><hi rend="capitalsize1">D</hi>attu die doot zult ontsien</l>
<lb n="19" xml:id="Pf10ra.19"/><l n="P_I,22_0003"><hi rend="capitalsize1">W</hi>ant wie die doot ontsiet</l>
<lb n="20" xml:id="Pf10ra.20"/><l n="P_I,22_0004"><hi rend="capitalsize1">H</hi>em en helpt <choice><abbr>zij<g ref="#bar"/></abbr><expan>zij<ex>n</ex></expan></choice> leuen niet</l>


<pb xml:id="P.f10vb" n="10vb"/>
</lg>
<lg type="disticha" n="I,19">
<lb n="1" xml:id="Pf10vb.1"/><l n="P_I,19_0001"><hi rend="capitalsize1">H</hi>ij is sotter dan queken hoot</l>
<lb n="2" xml:id="Pf10vb.2"/><l n="P_I,19_0002"><hi rend="capitalsize1">D</hi>ie hoopt vp eens <choice><abbr>a<g ref="#bar"/>ders</abbr><expan>a<ex>n</ex>ders</expan></choice> doot</l>
<lb n="3" xml:id="Pf10vb.3"/><l n="P_I,19_0003"><choice><abbr><hi rend="capitalsize1">W</hi>a<g ref="#bar"/>t</abbr><expan><hi rend="capitalsize1">W</hi>a<ex>n</ex>t</expan></choice> alle lieden jonc <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> hout</l>
<lb n="4" xml:id="Pf10vb.4"/><l n="P_I,19_0004"><hi rend="capitalsize1">Z</hi>ijns haers leuens onghewout</l>
</lg>
<lg type="disticha" n="I,24">
<lb n="5" xml:id="Pf10vb.5"/><l n="P_I,24_0001"><hi rend="capitalsize1">V</hi>an dien datti is ghebleuen</l>
<lb n="6" xml:id="Pf10vb.6"/><l n="P_I,24_0002"><hi rend="capitalsize1">Z</hi>ulstu behendelike leuen</l>
<lb n="7" xml:id="Pf10vb.7"/><l n="P_I,24_0003"><hi rend="capitalsize1">H</hi>out di <choice><abbr>e<g ref="#bar"/>mer</abbr><expan>e<ex>m</ex>mer</expan></choice> so ondersteken</l>
<lb n="8" xml:id="Pf10vb.8"/><l n="P_I,24_0004"><hi rend="capitalsize1">D</hi>at di niet en mach <choice><abbr>ghebreke<g ref="#bar"/></abbr><expan>ghebreke<ex>n</ex></expan></choice></l>
</lg>
<lg type="disticha" n="I,36">
<lb n="9" xml:id="Pf10vb.9"/><l n="P_I,36_0001"><hi rend="capitalsize1">I</hi>eghens die gone die <choice><abbr>mij<g ref="#bar"/>nen</abbr><expan>mij<ex>n</ex>nen</expan></choice></l>
<lb n="10" xml:id="Pf10vb.10"/><l n="P_I,36_0002"><hi rend="capitalsize1">E</hi>n zulstu sceldens niet <choice><abbr>beghy<g ref="#bar"/>ne<g ref="#bar"/></abbr><expan>beghy<ex>n</ex>ne<ex>n</ex></expan></choice></l>
<lb n="11" xml:id="Pf10vb.11"/><l n="P_I,36_0003"><choice><abbr><hi rend="capitalsize1">W</hi>a<g ref="#bar"/>t</abbr><expan><hi rend="capitalsize1">W</hi>a<ex>n</ex>t</expan></choice> <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> scelden hier te vooren</l>
<lb n="12" xml:id="Pf10vb.12"/><l n="P_I,36_0004"><hi rend="capitalsize1">V</hi>ernoy is <choice><abbr>co<g ref="#bar"/>men</abbr><expan>co<ex>m</ex>men</expan></choice> ende toren</l>


<pb xml:id="P.f11ra" n="11ra"/>
</lg>
<lg type="disticha" n="I,38">
<lb n="1" xml:id="Pf11ra.1"/><l n="P_I,38_0001"><hi rend="capitalsize1">A</hi>l mueghestu <choice><abbr>v<g ref="#apomod"/>winne<g ref="#bar"/></abbr><expan>v<ex>er</ex>winne<ex>n</ex></expan></choice> zaen</l>
<lb n="2" xml:id="Pf11ra.2"/><l n="P_I,38_0002"><choice><abbr><hi rend="capitalsize1">D</hi>ine<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">D</hi>ine<ex>n</ex></expan></choice> viant <choice><abbr>e<g ref="#bar"/>me</abbr><expan>e<ex>m</ex>me</expan></choice> <choice><abbr>late<g ref="#bar"/></abbr><expan>late<ex>n</ex></expan></choice> jn <choice><abbr>vrede<g ref="#bar"/></abbr><expan>vrede<ex>n</ex></expan></choice> <choice><abbr>stae<g ref="#bar"/></abbr><expan>stae<ex>n</ex></expan></choice></l>
<lb n="3" xml:id="Pf11ra.3"/><l n="P_I,38_0003"><hi rend="capitalsize1">I</hi>c zegghe di dat ghestadichede</l>
<lb n="4" xml:id="Pf11ra.4"/><l n="P_I,38_0004"><hi rend="capitalsize1">H</hi>euet meneghe goede zede</l>
</lg>
<lg type="disticha" n="I,39">
<lb n="5" xml:id="Pf11ra.5"/><l n="P_I,39_0001"><hi rend="capitalsize1">D</hi>at een man behaghet heift</l>
<lb n="6" xml:id="Pf11ra.6"/><l n="P_I,39_0002"><hi rend="capitalsize1">M</hi>et pinen <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> zottelike begheift</l>
<lb n="7" xml:id="Pf11ra.7"/><l n="P_I,39_0003"><hi rend="capitalsize1">D</hi>at lachtert alstu dit beuroet</l>
<lb n="8" xml:id="Pf11ra.8"/><l n="P_I,39_0004"><hi rend="capitalsize1">Z</hi>o keert daer of dinen moet</l>


<pb xml:id="P.f12vb" n="12vb"/>
</lg>
<lg type="disticha" n="I,40">
<lb n="1" xml:id="Pf12vb.1"/><l n="P_I,40_0001"><hi rend="capitalsize1">B</hi>eede nemen ende gheuen</l>
<lb n="2" xml:id="Pf12vb.2"/><l n="P_I,40_0002"><hi rend="capitalsize1">E</hi>s haer zede die nv leuen</l>
<lb n="3" xml:id="Pf12vb.3"/><l n="P_I,40_0003"><hi rend="capitalsize1">M</hi>et eeren zo doet also</l>
<lb n="4" xml:id="Pf12vb.4"/><l n="P_I,40_0004"><hi rend="capitalsize1">Z</hi>o muechstu blijde <choice><abbr>zij<g ref="#bar"/></abbr><expan>zij<ex>n</ex></expan></choice> <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> vro</l>
</lg>
<lg type="disticha" n="II,Pb">
<lb n="5" xml:id="Pf12vb.5"/><l n="P_II,Pb_0001"><hi rend="capitalsize1">W</hi>ilstu ooc leeren medecine</l>

<note n="P_II,Pb_0002">Line_number 0002 is missing</note>

<note n="P_II,Pb_0003">Line_number 0003 is missing</note>

<note n="P_II,Pb_0004">Line_number 0004 is missing</note>
</lg>
<lg type="disticha" n="II,Pd">

<note n="P_II,Pd_0001">Line_number 0001 is missing</note>
<lb n="6" xml:id="Pf12vb.6"/><l n="P_II,Pd_0002"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> <choice><abbr>mij<g ref="#bar"/>ne</abbr><expan>mij<ex>n</ex>ne</expan></choice> doen jn <choice><abbr>dine<g ref="#bar"/></abbr><expan>dine<ex>n</ex></expan></choice> <choice><abbr>zi<g ref="#bar"/>ne</abbr><expan>zi<ex>n</ex>ne</expan></choice></l>
<lb n="7" xml:id="Pf12vb.7"/><l n="P_II,Pd_0003"><hi rend="capitalsize1">O</hi>uidius leeret dij alsoo</l>
<lb n="8" xml:id="Pf12vb.8"/><l n="P_II,Pd_0004"><hi rend="capitalsize1">H</hi>oe du <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> <choice><abbr>mij<g ref="#bar"/>ne<g ref="#bar"/></abbr><expan>mij<ex>n</ex>ne<ex>n</ex></expan></choice> wordes vroo</l>
</lg>
<lg type="disticha" n="II,Pc">
<lb n="9" xml:id="Pf12vb.9"/><l n="P_II,Pc_0001"><hi rend="capitalsize1">W</hi>ilstu di dies onderwijnden</l>
<lb n="10" xml:id="Pf12vb.10"/><l n="P_II,Pc_0002"><hi rend="capitalsize1">D</hi>atstu gheuecht wilt <choice><abbr>beghi<g ref="#bar"/>nen</abbr><expan>beghi<ex>n</ex>nen</expan></choice></l>
