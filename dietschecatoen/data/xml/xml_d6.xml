    <text xml:id="DietscheCatoen">
        <group><pb xml:id="d6.fBiiijr" n="Biiijr"/>


<text n="prologue" xml:id="prologue">
<body>
<p><lg type="disticha" n="title">
<lb n="1" xml:id="d6fBiiijr.1"/><l n="d6_title_0001"><hi rend="capitalsize1">D</hi>it is de wjjse lee<num type="roman">ringhe</num> van <hi rend="capitalsize1">C</hi>atho.</l>
</lg>
<lg type="disticha" n="prologue">
<lb n="2" xml:id="d6fBiiijr.2"/><l n="d6_prologue_0001"><hi rend="capitalsize3">D</hi>Ie ghene die in haren sinne</l>
<lb n="3" xml:id="d6fBiiijr.3"/><l n="d6_prologue_0002"><hi rend="capitalsize1">D</hi>raghen weerlijcke minne</l>
<lb n="4" xml:id="d6fBiiijr.4"/><l n="d6_prologue_0003"><hi rend="capitalsize1">S</hi>y <choice><abbr>make<g ref="#bar"/></abbr><expan>make<ex>n</ex></expan></choice> daer af rijm en liet.</l>
<lb n="5" xml:id="d6fBiiijr.5"/><l n="d6_prologue_0004"><hi rend="capitalsize1">D</hi>er minnen soo en draghe ick niet</l>
<lb n="6" xml:id="d6fBiiijr.6"/><l n="d6_prologue_0005"><hi rend="capitalsize1">W</hi>ant nu is dat al vergaen</l>
<lb n="7" xml:id="d6fBiiijr.7"/><l n="d6_prologue_0006"><hi rend="capitalsize1">D</hi>at eer minne mach bestaen</l>
<lb n="8" xml:id="d6fBiiijr.8"/><l n="d6_prologue_0007"><hi rend="capitalsize1">W</hi>ijlen doen ick die minne droech</l>
<lb n="9" xml:id="d6fBiiijr.9"/><l n="d6_prologue_0008"><hi rend="capitalsize1">A</hi>ls ick die minne sach ick loech</l>
<lb n="10" xml:id="d6fBiiijr.10"/><l n="d6_prologue_0009"><hi rend="capitalsize1">N</hi>u haet ickse in alle mijnen sinne</l>
<lb n="11" xml:id="d6fBiiijr.11"/><l n="d6_prologue_0010">die minne draghen ende die minne</l>
<lb n="12" xml:id="d6fBiiijr.12"/><l n="d6_prologue_0011"><hi rend="capitalsize1">E</hi>nde heb ghekeert mijnen moet</l>
<lb n="13" xml:id="d6fBiiijr.13"/><l n="d6_prologue_0012"><hi rend="capitalsize1">A</hi>en die ghene die zijn vroet</l>
<lb n="14" xml:id="d6fBiiijr.14"/><l n="d6_prologue_0013"><hi rend="capitalsize1">E</hi>er ick dit dede seyde een mijn vrient</l>
<lb n="15" xml:id="d6fBiiijr.15"/><l n="d6_prologue_0014"><hi rend="capitalsize1">G</hi>heselle die ter wijsheyt dient</l>
<lb n="16" xml:id="d6fBiiijr.16"/><l n="d6_prologue_0015">dat hy des beteren loon ontfaet.</l>
<lb n="17" xml:id="d6fBiiijr.17"/><l n="d6_prologue_0016"><hi rend="capitalsize1">D</hi>an die ter minnen dienste staet</l>
<lb n="18" xml:id="d6fBiiijr.18"/><l n="d6_prologue_0017"><hi rend="capitalsize1">H</hi>ierom heb ick des begonnen</l>
<lb n="19" xml:id="d6fBiiijr.19"/><l n="d6_prologue_0018"><hi rend="capitalsize1">S</hi>y hebben onrecht dies my verionnen.</l>


<pb xml:id="d6.fiiijv" n="iiijv"/>
<lb n="1" xml:id="d6fiiijv.1"/><l n="d6_prologue_0019"><hi rend="capitalsize3">N</hi>V bidt alle met my te <hi rend="capitalsize1">G</hi>ode</l>
<lb n="2" xml:id="d6fiiijv.2"/><l n="d6_prologue_0020">dat hy seynde zijnen bode</l>
<lb n="3" xml:id="d6fiiijv.3"/><l n="d6_prologue_0021">die my wijse ende leere</l>
<lb n="4" xml:id="d6fiiijv.4"/><l n="d6_prologue_0022"><hi rend="capitalsize1">H</hi>oe ick my ter vroetheyt keere</l>
<lb n="5" xml:id="d6fiiijv.5"/><l n="d6_prologue_0023"><hi rend="capitalsize1">A</hi>lle die vroet willen wesen.</l>
<lb n="6" xml:id="d6fiiijv.6"/><l n="d6_prologue_0024"><hi rend="capitalsize1">E</hi>en boec is dat de <hi rend="capitalsize1">C</hi>lercken lesen</l>
<lb n="7" xml:id="d6fiiijv.7"/><l n="d6_prologue_0025"><hi rend="capitalsize1">A</hi>ls sy eerst ter scholen gaen</l>
<lb n="8" xml:id="d6fiiijv.8"/><l n="d6_prologue_0026">dat hen wijsheyt doet verstaen</l>
<lb n="9" xml:id="d6fiiijv.9"/><l n="d6_prologue_0027"><hi rend="capitalsize1">V</hi>e<unclear>el</unclear> meer dan eenich doet.</l>
<lb n="10" xml:id="d6fiiijv.10"/><l n="d6_prologue_0028"><hi rend="capitalsize1">H</hi>ie<unclear>rom</unclear> peysde ick in mijnen moet</l>
<lb n="11" xml:id="d6fiiijv.11"/><l n="d6_prologue_0029">dat <unclear>ic</unclear>k v daer by wil leeren</l>
<lb n="12" xml:id="d6fiiijv.12"/><l n="d6_prologue_0030"><hi rend="capitalsize1">H</hi>oe ghy u herte sult bekeeren</l>
<lb n="13" xml:id="d6fiiijv.13"/><l n="d6_prologue_0031"><hi rend="capitalsize1">T</hi>er vroetheyt nu mercket dan.</l>
<lb n="14" xml:id="d6fiiijv.14"/><l n="d6_prologue_0032"><hi rend="capitalsize1">W</hi>ient maeckte het woonde een man</l>
<lb n="15" xml:id="d6fiiijv.15"/><l n="d6_prologue_0033"><hi rend="capitalsize1">T</hi>e <hi rend="capitalsize1">R</hi>oomen wijlen eer</l>
<lb n="16" xml:id="d6fiiijv.16"/><l n="d6_prologue_0034">die des wijsdom wiste meer</l>
<lb n="17" xml:id="d6fiiijv.17"/><l n="d6_prologue_0035">dan eenich man die leuet nu.</l>
<lb n="18" xml:id="d6fiiijv.18"/><l n="d6_prologue_0036"><hi rend="capitalsize1">H</hi>y hiet <hi rend="capitalsize1">C</hi>atho dat segghe ick v</l>
<lb n="19" xml:id="d6fiiijv.19"/><l n="d6_prologue_0037"><hi rend="capitalsize1">H</hi>y hadde een kint dat alsoo</l>
<lb n="20" xml:id="d6fiiijv.20"/><l n="d6_prologue_0038"><hi rend="capitalsize1">A</hi>ls hijt noemde dies was hy vro</l>
<lb n="21" xml:id="d6fiiijv.21"/><l n="d6_prologue_0039"><hi rend="capitalsize1">A</hi>ls metten kinde was vergaen.</l>
<lb n="22" xml:id="d6fiiijv.22"/><l n="d6_prologue_0040"><hi rend="capitalsize1">D</hi>at mocht onthouden ende verstaen</l>
<lb n="23" xml:id="d6fiiijv.23"/><l n="d6_prologue_0041"><hi rend="capitalsize1">R</hi>iep hy ende sprack tot hem</l>
<lb n="24" xml:id="d6fiiijv.24"/><l n="d6_prologue_0042"><hi rend="capitalsize1">M</hi>en seyt sone dat ick ben</l>


<pb xml:id="d6.fvr" n="vr"/>
<lb n="1" xml:id="d6fvr.1"/><l n="d6_prologue_0043"><hi rend="capitalsize1">W</hi>ijs ende vroet nu merckt aen</l>
<lb n="2" xml:id="d6fvr.2"/><l n="d6_prologue_0044"><hi rend="capitalsize1">I</hi>ck sal v leeren dat ick can</l>
<lb n="3" xml:id="d6fvr.3"/><l n="d6_prologue_0045">doen seyde hy ende began aldus.</l>
<lb n="4" xml:id="d6fvr.4"/><l n="d6_prologue_0046"><hi rend="capitalsize1">S</hi>i <hi rend="capitalsize1">D</hi>eus est animus</l>
</lg>
</p>
</body>
</text>


<text n="disticha" xml:id="disticha">
<body>
<p><lg type="disticha" n="I,01">
<lb n="5" xml:id="d6fvr.5"/><l n="d6_I,01_0001"><hi rend="capitalsize1">N</hi>v merckt sone wat ick ghebiede</l>
<lb n="6" xml:id="d6fvr.6"/><l n="d6_I,01_0002"><hi rend="capitalsize1">E</hi>nde wat dat is dat ick v bediede</l>
<lb n="7" xml:id="d6fvr.7"/><l n="d6_I,01_0003"><hi rend="capitalsize1">D</hi>ats dat ghy v daer aen sult keeren</l>
<lb n="8" xml:id="d6fvr.8"/><l n="d6_I,01_0004"><hi rend="capitalsize1">D</hi>at ghy <hi rend="capitalsize1">G</hi>odt ymmer sult eeren.</l>
</lg>
<lg type="disticha" n="I,02">
<lb n="9" xml:id="d6fvr.9"/><l n="d6_I,02_0001"><hi rend="capitalsize1">G</hi>hy sult smorghens vroech opstaen</l>
<lb n="10" xml:id="d6fvr.10"/><l n="d6_I,02_0002"><hi rend="capitalsize1">E</hi>nde om v beerne gaen</l>
<lb n="11" xml:id="d6fvr.11"/><l n="d6_I,02_0003"><hi rend="capitalsize1">M</hi>en seyt die te langhe slaept</l>
<lb n="12" xml:id="d6fvr.12"/><l n="d6_I,02_0004"><hi rend="capitalsize1">D</hi>at hem den slaep onduchtich maeckt.</l>
</lg>
<lg type="disticha" n="I,03">
<lb n="13" xml:id="d6fvr.13"/><l n="d6_I,03_0001"><hi rend="capitalsize1">W</hi>acht v datmen niet en seyt</l>
<lb n="14" xml:id="d6fvr.14"/><l n="d6_I,03_0002"><hi rend="capitalsize1">D</hi>at vele tale aen v leyt</l>
<lb n="15" xml:id="d6fvr.15"/><l n="d6_I,03_0003"><hi rend="capitalsize1">W</hi>ant menigherande tale</l>
<lb n="16" xml:id="d6fvr.16"/><l n="d6_I,03_0004"><hi rend="capitalsize1">E</hi>n voecht niemant goeders wale.</l>
</lg>
<lg type="disticha" n="I,04">
<lb n="17" xml:id="d6fvr.17"/><l n="d6_I,04_0001"><hi rend="capitalsize1">S</hi>one doet als die vroede doet</l>
<lb n="18" xml:id="d6fvr.18"/><l n="d6_I,04_0002"><hi rend="capitalsize1">D</hi>ie heuet ghestadighen moet</l>
<lb n="19" xml:id="d6fvr.19"/><l n="d6_I,04_0003"><hi rend="capitalsize1">H</hi>et is lachter dat een hier seghet</l>
<lb n="20" xml:id="d6fvr.20"/><l n="d6_I,04_0004"><hi rend="capitalsize1">D</hi>at hy ginder niet en pleghet.</l>
</lg>
<lg type="disticha" n="I,05">
<lb n="21" xml:id="d6fvr.21"/><l n="d6_I,05_0001"><hi rend="capitalsize1">A</hi>enmerct niemants quade daet</l>
<lb n="22" xml:id="d6fvr.22"/><l n="d6_I,05_0002"><hi rend="capitalsize1">W</hi>ant veel merckens is quaet</l>
<lb n="23" xml:id="d6fvr.23"/><l n="d6_I,05_0003"><hi rend="capitalsize1">B</hi>eyde soo en is man oft wijf</l>
<lb n="24" xml:id="d6fvr.24"/><l n="d6_I,05_0004"><hi rend="capitalsize1">D</hi>ie sonder sonde leyt zijn lijf.</l>


<pb xml:id="d6.fvv" n="vv"/>
</lg>
<lg type="disticha" n="I,08">
<lb n="1" xml:id="d6fvv.1"/><l n="d6_I,08_0001"><hi rend="capitalsize1">G</hi>helooft niet als v wijf tonrecht</l>
<lb n="2" xml:id="d6fvv.2"/><l n="d6_I,08_0002"><hi rend="capitalsize1">C</hi>laghet ouer uwen knecht</l>
<lb n="3" xml:id="d6fvv.3"/><l n="d6_I,08_0003"><hi rend="capitalsize1">W</hi>ant quade wijuen zijn ghenoech</l>
<lb n="4" xml:id="d6fvv.4"/><l n="d6_I,08_0004"><hi rend="capitalsize1">D</hi>ie dicke maken onghevoech.</l>
</lg>
<lg type="disticha" n="I,12">
<lb n="5" xml:id="d6fvv.5"/><l n="d6_I,12_0001"><hi rend="capitalsize1">N</hi>ieumaren ende achterspraken</l>
<lb n="6" xml:id="d6fvv.6"/><l n="d6_I,12_0002"><hi rend="capitalsize1">D</hi>ie de lieden tot schande maken</l>
<lb n="7" xml:id="d6fvv.7"/><l n="d6_I,12_0003"><hi rend="capitalsize1">S</hi>ult ghy schouwen ende haten</l>
<lb n="8" xml:id="d6fvv.8"/><l n="d6_I,12_0004"><hi rend="capitalsize1">D</hi>ie hem anders verwaten.</l>
</lg>
<lg type="disticha" n="I,15">
<lb n="9" xml:id="d6fvv.9"/><l n="d6_I,15_0001"><hi rend="capitalsize1">A</hi>ls eenich man doet wel</l>
<lb n="10" xml:id="d6fvv.10"/><l n="d6_I,15_0002"><hi rend="capitalsize1">D</hi>at onthout ende vertelt</l>
<lb n="11" xml:id="d6fvv.11"/><l n="d6_I,15_0003"><hi rend="capitalsize1">D</hi>oedy wel swijghet daer of</l>
<lb n="12" xml:id="d6fvv.12"/><l n="d6_I,15_0004"><hi rend="capitalsize1">S</hi>oo sult ghy hebben goeden lof.</l>
</lg>
<lg type="disticha" n="I,16">
<lb n="13" xml:id="d6fvv.13"/><l n="d6_I,16_0001"><hi rend="capitalsize1">D</hi>ie wijle ghy sijt een ionghelinck</l>
<lb n="14" xml:id="d6fvv.14"/><l n="d6_I,16_0002"><hi rend="capitalsize1">S</hi>oo doet soo menich goet dinck</l>
<lb n="15" xml:id="d6fvv.15"/><l n="d6_I,16_0003"><hi rend="capitalsize1">A</hi>ls dan naken uwe daghe</l>
<lb n="16" xml:id="d6fvv.16"/><l n="d6_I,16_0004"><hi rend="capitalsize1">D</hi>atmen ws te <hi rend="capitalsize1">G</hi>ode ghewaghe</l>
</lg>
<lg type="disticha" n="I,18">
<lb n="17" xml:id="d6fvv.17"/><l n="d6_I,18_0001"><hi rend="capitalsize1">A</hi>lst comt dat ghy hebt ghenoech</l>
<lb n="18" xml:id="d6fvv.18"/><l n="d6_I,18_0002"><hi rend="capitalsize1">V</hi>an hauen hout u in dit gheuoech/</l>
<lb n="19" xml:id="d6fvv.19"/><l n="d6_I,18_0003"><hi rend="capitalsize1">W</hi>ant heden is die man rijcke</l>
<lb n="20" xml:id="d6fvv.20"/><l n="d6_I,18_0004"><hi rend="capitalsize1">S</hi>morghens staet hem commerlijcke.</l>
</lg>
<lg type="disticha" n="I,21">
<lb n="21" xml:id="d6fvv.21"/><l n="d6_I,21_0001"><hi rend="capitalsize1">A</hi>ls ghy arm zijt van hauen</l>
<lb n="22" xml:id="d6fvv.22"/><l n="d6_I,21_0002"><hi rend="capitalsize1">E</hi>nde van rijcdommen beschauen</l>
<lb n="23" xml:id="d6fvv.23"/><l n="d6_I,21_0003"><hi rend="capitalsize1">B</hi>epeyst dat ghy hier te voren</l>
<lb n="24" xml:id="d6fvv.24"/><l n="d6_I,21_0004"><hi rend="capitalsize1">A</hi>rm ende naeckt waert gheboren.</l>


<pb xml:id="d6.fvjr" n="vjr"/>
</lg>
<lg type="disticha" n="I,22">
<lb n="1" xml:id="d6fvjr.1"/><l n="d6_I,22_0001"><hi rend="capitalsize1">G</hi>hy sult niet keeren in dien</l>
<lb n="2" xml:id="d6fvjr.2"/><l n="d6_I,22_0002"><hi rend="capitalsize1">D</hi>at ghy die doot sult ontsien</l>
<lb n="3" xml:id="d6fvjr.3"/><l n="d6_I,22_0003"><hi rend="capitalsize1">W</hi>ant wie die doot ontsiet</l>
<lb n="4" xml:id="d6fvjr.4"/><l n="d6_I,22_0004"><hi rend="capitalsize1">H</hi>em en helpt zijn leuen niet.</l>
</lg>
<lg type="disticha" n="I,19">
<lb n="5" xml:id="d6fvjr.5"/><l n="d6_I,19_0001"><hi rend="capitalsize1">H</hi>y is sotter dan een queken hoot</l>
<lb n="6" xml:id="d6fvjr.6"/><l n="d6_I,19_0002"><hi rend="capitalsize1">D</hi>ie hopet op een anders doot</l>
<lb n="7" xml:id="d6fvjr.7"/><l n="d6_I,19_0003"><hi rend="capitalsize1">W</hi>ant beyde die lieden ionck en out</l>
<lb n="8" xml:id="d6fvjr.8"/><l n="d6_I,19_0004"><hi rend="capitalsize1">S</hi>ijn haers leuens onghewout.</l>
</lg>
<lg type="disticha" n="I,23">
<lb n="9" xml:id="d6fvjr.9"/><l n="d6_I,23_0001"><hi rend="capitalsize1">A</hi>l hebdy dat verdient</l>
<lb n="10" xml:id="d6fvjr.10"/><l n="d6_I,23_0002"><hi rend="capitalsize1">D</hi>at ghy en behoeft gheenen vrient</l>
<lb n="11" xml:id="d6fvjr.11"/><l n="d6_I,23_0003"><hi rend="capitalsize1">D</hi>at en suldy <hi rend="capitalsize1">G</hi>odt wijten niet</l>
<lb n="12" xml:id="d6fvjr.12"/><l n="d6_I,23_0004"><hi rend="capitalsize1">W</hi>ant hem is leet dat u misschiet.</l>
</lg>
<lg type="disticha" n="I,26">
<lb n="13" xml:id="d6fvjr.13"/><l n="d6_I,26_0001"><hi rend="capitalsize1">G</hi>hy sult v wachten doedy wale</l>
<lb n="14" xml:id="d6fvjr.14"/><l n="d6_I,26_0002"><hi rend="capitalsize1">T</hi>eghen hem te hebben tale</l>
<lb n="15" xml:id="d6fvjr.15"/><l n="d6_I,26_0003"><hi rend="capitalsize1">D</hi>ie altoos zijn beuaen in dien</l>
<lb n="16" xml:id="d6fvjr.16"/><l n="d6_I,26_0004"><hi rend="capitalsize1">H</hi>oe sy verradenis moghen plien</l>
</lg>
<lg type="disticha" n="I,27">
<lb n="17" xml:id="d6fvjr.17"/><l n="d6_I,27_0001"><hi rend="capitalsize1">V</hi>vacht die teghen v ghebaren</l>
<lb n="18" xml:id="d6fvjr.18"/><l n="d6_I,27_0002"><hi rend="capitalsize1">M</hi>innelijck als die <hi rend="capitalsize1">V</hi>oghelaren</l>
<lb n="19" xml:id="d6fvjr.19"/><l n="d6_I,27_0003"><hi rend="capitalsize1">D</hi>ie schoon pijpen ende blasen</l>
<lb n="20" xml:id="d6fvjr.20"/><l n="d6_I,27_0004"><hi rend="capitalsize1">A</hi>ls die <hi rend="capitalsize1">V</hi>oghelkens verdwasen.</l>
</lg>
<lg type="disticha" n="III,11">
<lb n="21" xml:id="d6fvjr.21"/><l n="d6_III,11_0001"><hi rend="capitalsize1">N</hi>ae dat uwe tijden naken</l>
<lb n="22" xml:id="d6fvjr.22"/><l n="d6_III,11_0002"><hi rend="capitalsize1">S</hi>uldy v teringhe maken</l>
<lb n="23" xml:id="d6fvjr.23"/><l n="d6_III,11_0003"><hi rend="capitalsize1">E</hi>nde uwen cost meer noch min</l>
<lb n="24" xml:id="d6fvjr.24"/><l n="d6_III,11_0004"><hi rend="capitalsize1">N</hi>a dien dat ghy hebt ghewin.</l>


<pb xml:id="d6.fvjv" n="vjv"/>
</lg>
<lg type="disticha" n="IV,04">
<lb n="1" xml:id="d6fvjv.1"/><l n="d6_IV,04_0001"><hi rend="capitalsize1">G</hi>hy moghet ghelt winnen wel</l>
<lb n="2" xml:id="d6fvjv.2"/><l n="d6_IV,04_0002"><hi rend="capitalsize1">D</hi>oor nutschap ende niet el</l>
<lb n="3" xml:id="d6fvjv.3"/><l n="d6_IV,04_0003"><hi rend="capitalsize1">M</hi>en seyt daer noyt heylich man/</l>
<lb n="4" xml:id="d6fvjv.4"/><l n="d6_IV,04_0004"><hi rend="capitalsize1">E</hi>n leyden zijnen sin daer aen.</l>
</lg>
<lg type="disticha" n="I,40">
<lb n="5" xml:id="d6fvjv.5"/><l n="d6_I,40_0001"><hi rend="capitalsize1">B</hi>eyde nemen ende gheuen</l>
<lb n="6" xml:id="d6fvjv.6"/><l n="d6_I,40_0002"><hi rend="capitalsize1">I</hi>s haer sede die nu wil leuen</l>
<lb n="7" xml:id="d6fvjv.7"/><l n="d6_I,40_0003"><hi rend="capitalsize1">M</hi>et eeren/sone doet alsoo</l>
<lb n="8" xml:id="d6fvjv.8"/><l n="d6_I,40_0004"><hi rend="capitalsize1">S</hi>oo meuchdy blijde zijn ende vro.</l>
</lg>
<lg type="disticha" n="II,Pa">
<lb n="9" xml:id="d6fvjv.9"/><l n="d6_II,Pa_0001"><hi rend="capitalsize1">W</hi>ildy sone daer toe keeren</l>
<lb n="10" xml:id="d6fvjv.10"/><l n="d6_II,Pa_0002"><hi rend="capitalsize1">V</hi>irgilius heuet beschreuen</l>
<lb n="11" xml:id="d6fvjv.11"/><l n="d6_II,Pa_0003"><hi rend="capitalsize1">H</hi>oe die dorpers sullen leuen.</l>

<note n="d6_II,Pa_0004">Line_number 0004 is missing</note>
</lg>
<lg type="disticha" n="II,06">
<lb n="12" xml:id="d6fvjv.12"/><l n="d6_II,06_0001"><hi rend="capitalsize1">G</hi>hy sult de onmate vlien</l>
<lb n="13" xml:id="d6fvjv.13"/><l n="d6_II,06_0002"><hi rend="capitalsize1">M</hi>en seyt dat vroede plien</l>
<lb n="14" xml:id="d6fvjv.14"/><l n="d6_II,06_0003"><hi rend="capitalsize1">M</hi>ate te houden/ ende volghen hare</l>
<lb n="15" xml:id="d6fvjv.15"/><l n="d6_II,06_0004"><hi rend="capitalsize1">M</hi>ate/onmate is ongheware.</l>
</lg>
<lg type="disticha" n="II,07">
<lb n="16" xml:id="d6fvjv.16"/><l n="d6_II,07_0001"><hi rend="capitalsize1">S</hi>egdy ws ghesellen daet</l>
<lb n="17" xml:id="d6fvjv.17"/><l n="d6_II,07_0002"><hi rend="capitalsize1">D</hi>ies hy hem schaemt dat is quaet</l>
<lb n="18" xml:id="d6fvjv.18"/><l n="d6_II,07_0003"><hi rend="capitalsize1">P</hi>eyst oft het v lief ware</l>
<lb n="19" xml:id="d6fvjv.19"/><l n="d6_II,07_0004"><hi rend="capitalsize1">D</hi>ie uwe schande maeckt openbare.</l>
</lg>
<lg type="disticha" n="II,08">
<lb n="20" xml:id="d6fvjv.20"/><l n="d6_II,08_0001"><hi rend="capitalsize1">S</hi>one en merckt die sonde niet</l>
<lb n="21" xml:id="d6fvjv.21"/><l n="d6_II,08_0002"><hi rend="capitalsize1">D</hi>iemen den quaden doen siet</l>
<lb n="22" xml:id="d6fvjv.22"/><l n="d6_II,08_0003"><hi rend="capitalsize1">W</hi>ant den tijt sal comen saen</l>
<lb n="23" xml:id="d6fvjv.23"/><l n="d6_II,08_0004"><hi rend="capitalsize1">D</hi>at zijs loon sullen ontfaen.</l>
</lg>
<lg type="disticha" n="II,13">
<lb n="24" xml:id="d6fvjv.24"/><l n="d6_II,13_0001"><hi rend="capitalsize1">S</hi>one ick verbiede v nijdichede</l>


<pb xml:id="d6.fvijr" n="vijr"/>
<lb n="1" xml:id="d6fvijr.1"/><l n="d6_II,13_0002"><hi rend="capitalsize1">W</hi>ant sy heuet quade zede</l>
<lb n="2" xml:id="d6fvijr.2"/><l n="d6_II,13_0003"><hi rend="capitalsize1">W</hi>ie datse draghet <hi rend="capitalsize1">G</hi>odt weet</l>
<lb n="3" xml:id="d6fvijr.3"/><l n="d6_II,13_0004"><hi rend="capitalsize1">H</hi>em comter af groot leet.</l>
</lg>
<lg type="disticha" n="III,16">
<lb n="4" xml:id="d6fvijr.4"/><l n="d6_III,16_0001"><hi rend="capitalsize1">G</hi>hy sult hebben vasten moet/</l>
<lb n="5" xml:id="d6fvijr.5"/><l n="d6_III,16_0002"><hi rend="capitalsize1">W</hi>anneermen v onrecht doet</l>
<lb n="6" xml:id="d6fvijr.6"/><l n="d6_III,16_0003"><hi rend="capitalsize1">S</hi>oo bidt den rechter dat hy dy</l>
<lb n="7" xml:id="d6fvijr.7"/><l n="d6_III,16_0004"><hi rend="capitalsize1">D</hi>oor uwen dienst behulpich sy.</l>
</lg>
<lg type="disticha" n="II,21">
<lb n="8" xml:id="d6fvijr.8"/><l n="d6_II,21_0001"><hi rend="capitalsize1">D</hi>at v in dronckenschap gheschiet</l>
<lb n="9" xml:id="d6fvijr.9"/><l n="d6_II,21_0002"><hi rend="capitalsize1">D</hi>at en laet onghebetert niet</l>
<lb n="10" xml:id="d6fvijr.10"/><l n="d6_II,21_0003"><hi rend="capitalsize1">W</hi>anter af hier te voren</l>
<lb n="11" xml:id="d6fvijr.11"/><l n="d6_II,21_0004"><hi rend="capitalsize1">V</hi>ernoy is comen ende toren</l>
</lg>
<lg type="disticha" n="II,17">
<lb n="12" xml:id="d6fvijr.12"/><l n="d6_II,17_0001"><hi rend="capitalsize1">A</hi>ls ghy hebt den cost weer</l>
<lb n="13" xml:id="d6fvijr.13"/><l n="d6_II,17_0002"><hi rend="capitalsize1">E</hi>nde teerens dan ghy hadt eer</l>
<lb n="14" xml:id="d6fvijr.14"/><l n="d6_II,17_0003"><hi rend="capitalsize1">S</hi>oo spaert/ want saen ghelijdet</l>
<lb n="15" xml:id="d6fvijr.15"/><l n="d6_II,17_0004"><hi rend="capitalsize1">H</hi>auen diemen niet en mijdet.</l>
</lg>
<lg type="disticha" n="II,24">
<lb n="16" xml:id="d6fvijr.16"/><l n="d6_II,24_0001"><hi rend="capitalsize1">M</hi>erckt wel dat mach v deeren/</l>
<lb n="17" xml:id="d6fvijr.17"/><l n="d6_II,24_0002">bewaert oft ghy moghet beweeren</l>
<lb n="18" xml:id="d6fvijr.18"/><l n="d6_II,24_0003"><hi rend="capitalsize1">H</hi>y is sot die comen siet</l>
<lb n="19" xml:id="d6fvijr.19"/><l n="d6_II,24_0004"><hi rend="capitalsize1">H</hi>et ongheval en niet en vliet.</l>
</lg>
<lg type="disticha" n="II,26">
<lb n="20" xml:id="d6fvijr.20"/><l n="d6_II,26_0001"><hi rend="capitalsize1">D</hi>ie wat heuet dat hem becomt</l>
<lb n="21" xml:id="d6fvijr.21"/><l n="d6_II,26_0002"><hi rend="capitalsize1">H</hi>out hyt wel ick waent hem vroemt</l>
<lb n="22" xml:id="d6fvijr.22"/><l n="d6_II,26_0003"><hi rend="capitalsize1">H</hi>y is sot die in zijn hant heeft goet</l>
<lb n="23" xml:id="d6fvijr.23"/><l n="d6_II,26_0004"><hi rend="capitalsize1">E</hi>nde leyt dat onder zijnen voet.</l>
</lg>
<lg type="disticha" n="II,28">
<lb n="24" xml:id="d6fvijr.24"/><l n="d6_II,28_0001"><hi rend="capitalsize1">S</hi>omwijlen sult ghy v goet</l>


<pb xml:id="d6.fvijv" n="vijv"/>
<lb n="1" xml:id="d6fvijv.1"/><l n="d6_II,28_0002"><hi rend="capitalsize1">S</hi>paren als die vroede doet</l>
<lb n="2" xml:id="d6fvijv.2"/><l n="d6_II,28_0003"><hi rend="capitalsize1">D</hi>ie soeckt altoos sulcke gheclach</l>
<lb n="3" xml:id="d6fvijv.3"/><l n="d6_II,28_0004"><hi rend="capitalsize1">H</hi>oe hy met eeren leuen mach.</l>
</lg>
<lg type="disticha" n="II,29">
<lb n="4" xml:id="d6fvijv.4"/><l n="d6_II,29_0001"><hi rend="capitalsize1">G</hi>hy en sult alleene niet ontslaen</l>
<lb n="5" xml:id="d6fvijv.5"/><l n="d6_II,29_0002"><hi rend="capitalsize1">T</hi>'recht dat langhe heeft ghestaen</l>
<lb n="6" xml:id="d6fvijv.6"/><l n="d6_II,29_0003">die verweckt zijnen ghebuere</l>
<lb n="7" xml:id="d6fvijv.7"/><l n="d6_II,29_0004"><hi rend="capitalsize1">H</hi>et wort hem dicwils al te suere.</l>
</lg>
<lg type="disticha" n="III,01">
<lb n="8" xml:id="d6fvijv.8"/><l n="d6_III,01_0001"><hi rend="capitalsize1">S</hi>one leert in uwen sin</l>
<lb n="9" xml:id="d6fvijv.9"/><l n="d6_III,01_0002">dat daer woone wijsheyt in</l>
<lb n="10" xml:id="d6fvijv.10"/><l n="d6_III,01_0003"><hi rend="capitalsize1">D</hi>ie met sonden leyt zijn leuen</l>
<lb n="11" xml:id="d6fvijv.11"/><l n="d6_III,01_0004"><hi rend="capitalsize1">H</hi>et is qualijcken met hem becleuen.</l>
</lg>
<lg type="disticha" n="III,21">
<lb n="12" xml:id="d6fvijv.12"/><l n="d6_III,21_0001"><hi rend="capitalsize1">S</hi>one oft ghy vroet wilt zijn</l>
<lb n="13" xml:id="d6fvijv.13"/><l n="d6_III,21_0002"><hi rend="capitalsize1">S</hi>oo t'herte in goeder maten dijn</l>
<lb n="14" xml:id="d6fvijv.14"/><l n="d6_III,21_0003"><hi rend="capitalsize1">H</hi>y is sot diet zijne verteert</l>
<lb n="15" xml:id="d6fvijv.15"/><l n="d6_III,21_0004"><hi rend="capitalsize1">E</hi>nde aen den vremden hem gheneert.</l>
</lg>
<lg type="disticha" n="III,21bis">
<lb n="16" xml:id="d6fvijv.16"/><l n="d6_III,21bis_0001"><hi rend="capitalsize1">L</hi>ieue sone wacht v dies</l>
<lb n="17" xml:id="d6fvijv.17"/><l n="d6_III,21bis_0002">dat ghy biddet niet en plies</l>
<lb n="18" xml:id="d6fvijv.18"/><l n="d6_III,21bis_0003"><hi rend="capitalsize1">I</hi>ck segt v hoet int lant steet</l>
<lb n="19" xml:id="d6fvijv.19"/><l n="d6_III,21bis_0004"><hi rend="capitalsize1">D</hi>ie vele bidt hy worde leet.</l>
</lg>
<lg type="disticha" n="III,22">
<lb n="20" xml:id="d6fvijv.20"/><l n="d6_III,22_0001"><hi rend="capitalsize1">D</hi>ie doot en suldy haten niet</l>
<lb n="21" xml:id="d6fvijv.21"/><l n="d6_III,22_0002"><hi rend="capitalsize1">D</hi>oor een pijne die soo vliet</l>
<lb n="22" xml:id="d6fvijv.22"/><l n="d6_III,22_0003">die sy recht ouer die quade</l>
<lb n="23" xml:id="d6fvijv.23"/><l n="d6_III,22_0004"><hi rend="capitalsize1">A</hi>l comt hy dicke te spade.</l>
</lg>
<lg type="disticha" n="III,24">
<lb n="24" xml:id="d6fvijv.24"/><l n="d6_III,24_0001"><hi rend="capitalsize1">G</hi>hy sult met uwen sin allegader</l>


<pb xml:id="d6.fviijr" n="viijr"/>
<lb n="1" xml:id="d6fviijr.1"/><l n="d6_III,24_0002"><hi rend="capitalsize1">E</hi>eren <hi rend="capitalsize1">M</hi>oeder ende <hi rend="capitalsize1">V</hi>ader</l>
<lb n="2" xml:id="d6fviijr.2"/><l n="d6_III,24_0003"><hi rend="capitalsize1">G</hi>odt seyt diese belghen doet</l>
<lb n="3" xml:id="d6fviijr.3"/><l n="d6_III,24_0004"><hi rend="capitalsize1">D</hi>at hem te pijne worden moet.</l>
</lg>
<lg type="disticha" n="independent stanza">
<lb n="4" xml:id="d6fviijr.4"/><l n="d6_independent stanza_0001"><hi rend="capitalsize1">S</hi>one dit is den besten schat</l>
<lb n="5" xml:id="d6fviijr.5"/><l n="d6_independent stanza_0002"><hi rend="capitalsize1">D</hi>ie ick hebbe/ende om dat</l>
<lb n="6" xml:id="d6fviijr.6"/><l n="d6_independent stanza_0003"><hi rend="capitalsize1">S</hi>oo heet ick ende beuele dy</l>
<lb n="7" xml:id="d6fviijr.7"/><l n="d6_independent stanza_0004"><hi rend="capitalsize1">D</hi>at hy altoos met v sy.</l>
</lg>
</p>
</body>
</text>


<text n="epilogue" xml:id="epilogue">
<body>
<p><lg type="disticha" n="epilogue">
<lb n="8" xml:id="d6fviijr.8"/><l n="d6_epilogue_0001"><hi rend="capitalsize1">G</hi>hy goede lieden die dese reden</l>
<lb n="9" xml:id="d6fviijr.9"/><l n="d6_epilogue_0002"><hi rend="capitalsize1">H</hi>ebt ghehoort doet v ghebeden</l>
<lb n="10" xml:id="d6fviijr.10"/><l n="d6_epilogue_0003"><hi rend="capitalsize1">O</hi>otmoedelijck tot onsen <hi rend="capitalsize1">H</hi>eere</l>
<lb n="11" xml:id="d6fviijr.11"/><l n="d6_epilogue_0004"><hi rend="capitalsize1">B</hi>idt hem dat hy v leere</l>
<lb n="12" xml:id="d6fviijr.12"/><l n="d6_epilogue_0005"><hi rend="capitalsize1">E</hi>nde lichten soo uwen sin</l>
<lb n="13" xml:id="d6fviijr.13"/><l n="d6_epilogue_0006">dat daer woone wijsheyt in.</l>
<lb n="14" xml:id="d6fviijr.14"/><l n="d6_epilogue_0007"><hi rend="capitalsize1">D</hi>ie dit maeckten wten <hi rend="capitalsize1">L</hi>atijne</l>
<lb n="15" xml:id="d6fviijr.15"/><l n="d6_epilogue_0008"><hi rend="capitalsize1">I</hi>n duytsche/begheert te zijne</l>
<lb n="16" xml:id="d6fviijr.16"/><l n="d6_epilogue_0009"><hi rend="capitalsize1">I</hi>n v ghebeden vroech ende spade</l>
<lb n="17" xml:id="d6fviijr.17"/><l n="d6_epilogue_0010"><hi rend="capitalsize1">B</hi>idt <hi rend="capitalsize1">G</hi>odt dat hy hem doe ghenade.</l>

<note n="d6_epilogue_0011">Line_number 0011 is missing</note>

<note n="d6_epilogue_0012">Line_number 0012 is missing</note>

<note n="d6_epilogue_0013">Line_number 0013 is missing</note>

<note n="d6_epilogue_0014">Line_number 0014 is missing</note>

<note n="d6_epilogue_0015">Line_number 0015 is missing</note>
<lb n="18" xml:id="d6fviijr.18"/><l n="d6_epilogue_0016"><hi rend="capitalsize1">F</hi>INIS.</l>
</lg>
<lg type="disticha" n="approbation">
<lb n="19" xml:id="d6fviijr.19"/><l n="d6_approbation_0001"><hi rend="capitalsize1">D</hi>it <hi rend="capitalsize1">B</hi>oecxken is gheuisiteert ende geapprobeert byden // eerweerdigen <hi rend="capitalsize1">H</hi>eer <hi rend="capitalsize1">S</hi>ymon de <hi rend="capitalsize1">P</hi>lanen.</l>
</lg>
</p>
</body>
