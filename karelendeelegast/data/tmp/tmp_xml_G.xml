    <text xml:id="Elegast">
        <group><text n="Karel ende Elegast" xml:id="KE">
<body>
<p>

<pb xml:id="G.f15r" n="15r"/>
<lb n="1" xml:id="Gf15r.1"/><l n="G_KE_0736"><hi rend="capitalsize1">S</hi>prach elegast hir wil ich <damage><gap></gap></damage></l>
<lb n="2" xml:id="Gf15r.2"/><l n="G_KE_0737"><hi rend="capitalsize1">N</hi>u besijet seit he aelbrecht</l>
<lb n="3" xml:id="Gf15r.3"/><l n="G_KE_0738"><hi rend="capitalsize1">W</hi>at dunct .v. hie gedaen recht</l>
<lb n="4" xml:id="Gf15r.4"/><l n="G_KE_0739"><hi rend="capitalsize1">I</hi>ch wil <choice><abbr>wijrcke<g ref="#bar"/></abbr><expan>wijrcke<ex>n</ex></expan></choice> bi vren rade</l>
<lb n="5" xml:id="Gf15r.5"/><l n="G_KE_0740"><hi rend="capitalsize1">M</hi>ir were leit geschiede ons schade</l>
<lb n="6" xml:id="Gf15r.6"/><l n="G_KE_0741"><hi rend="capitalsize1">D</hi>at wir nit <choice><abbr>mochte<g ref="#bar"/></abbr><expan>mochte<ex>n</ex></expan></choice> <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> dan</l>
<lb n="7" xml:id="Gf15r.7"/><l n="G_KE_0742"><hi rend="capitalsize1">E</hi>t quam al bi desen man</l>
<lb n="8" xml:id="Gf15r.8"/><l n="G_KE_0743"><hi rend="capitalsize1">D</hi>ie <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> antworde nae der talen</l>
<lb n="9" xml:id="Gf15r.9"/><l n="G_KE_0744"><hi rend="capitalsize1">I</hi>ch in quam nie in den sale</l>
<lb n="10" xml:id="Gf15r.10"/><l n="G_KE_0745"><hi rend="capitalsize1">N</hi>och <choice><abbr>binne<g ref="#bar"/></abbr><expan>binne<ex>n</ex></expan></choice> den houe dat ich weit</l>
<lb n="11" xml:id="Gf15r.11"/><l n="G_KE_0746"><hi rend="capitalsize1">M</hi>ir soude sin onghereit</l>
<lb n="12" xml:id="Gf15r.12"/><l n="G_KE_0747"><hi rend="capitalsize1">S</hi>oude ich nu <choice><abbr>binne<g ref="#bar"/></abbr><expan>binne<ex>n</ex></expan></choice> gaen</l>
<lb n="13" xml:id="Gf15r.13"/><l n="G_KE_0748"><hi rend="capitalsize1">D</hi>at mut an v. seluer staen</l>
<lb n="14" xml:id="Gf15r.14"/><l n="G_KE_0749"><hi rend="capitalsize1">E</hi>legast seide. et is mir lijef</l>
<lb n="15" xml:id="Gf15r.15"/><l n="G_KE_0750"><hi rend="capitalsize1">S</hi>idij. ein behendich dief</l>
<lb n="16" xml:id="Gf15r.16"/><l n="G_KE_0751"><hi rend="capitalsize1">D</hi>at sal ich curt verstaen</l>
<lb n="17" xml:id="Gf15r.17"/><l n="G_KE_0752"><hi rend="capitalsize1">L</hi>aet ons ein gaet <choice><abbr>macke<g ref="#bar"/></abbr><expan>macke<ex>n</ex></expan></choice> gaen.</l>
<lb n="18" xml:id="Gf15r.18"/><l n="G_KE_0753"><hi rend="capitalsize1">I</hi>n die mure ter goeder vruch</l>
<lb n="19" xml:id="Gf15r.19"/><l n="G_KE_0754"><hi rend="capitalsize1">D</hi>o wir crupen sullen durch</l>
<lb n="20" xml:id="Gf15r.20"/><l n="G_KE_0755"><hi rend="capitalsize2">D</hi>at lofden si beeide well</l>
<lb n="21" xml:id="Gf15r.21"/><l n="G_KE_0756"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> bunden <damage><gap></gap></damage> snell</l>


<pb xml:id="G.f15v" n="15v"/>
<lb n="1" xml:id="Gf15v.1"/><l n="G_KE_0761"><hi rend="capitalsize1">D</hi>at couter <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> den pluech</l>
<lb n="2" xml:id="Gf15v.2"/><l n="G_KE_0762"><hi rend="capitalsize1">D</hi>o stunt <hi rend="capitalsize1">E</hi>legast <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> luch</l>
<lb n="3" xml:id="Gf15v.3"/><l n="G_KE_0763"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> vraegede. wo he dat dede <choice><abbr>macke<g ref="#bar"/></abbr><expan>macke<ex>n</ex></expan></choice></l>
<lb n="4" xml:id="Gf15v.4"/><l n="G_KE_0764"><hi rend="capitalsize1">C</hi>onste ich smijts huys geracken</l>
<lb n="5" xml:id="Gf15v.5"/><l n="G_KE_0765"><hi rend="capitalsize1">I</hi>ch deede macken sullich ein</l>
<lb n="6" xml:id="Gf15v.6"/><l n="G_KE_0766"><hi rend="capitalsize1">I</hi>n sach nie sullich ein</l>
<lb n="7" xml:id="Gf15v.7"/><l n="G_KE_0767"><hi rend="capitalsize1">G</hi>emacht te sulcken <choice><abbr>stijcke<g ref="#bar"/></abbr><expan>stijcke<ex>n</ex></expan></choice></l>
<lb n="8" xml:id="Gf15v.8"/><l n="G_KE_0768"><hi rend="capitalsize1">D</hi>ar men soude dor bycken</l>
<lb n="9" xml:id="Gf15v.9"/><l n="G_KE_0769"><hi rend="capitalsize1">D</hi>er <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> seide. it mach wal sijn</l>
<lb n="10" xml:id="Gf15v.10"/><l n="G_KE_0770"><hi rend="capitalsize1">I</hi>ch <choice><abbr>qua<g ref="#bar"/></abbr><expan>qua<ex>m</ex></expan></choice> gereeden vp den rijn</l>
<lb n="11" xml:id="Gf15v.11"/><l n="G_KE_0771"><hi rend="capitalsize1">D</hi>at leit ich <choice><abbr>me<g ref="#bar"/>nigen</abbr><expan>me<ex>n</ex>nigen</expan></choice> dach</l>
<lb n="12" xml:id="Gf15v.12"/><l n="G_KE_0772"><hi rend="capitalsize1">D</hi>at ich voer om min beiach</l>
<lb n="13" xml:id="Gf15v.13"/><l n="G_KE_0773"><hi rend="capitalsize1">D</hi>ar muest ich min yser laeten</l>
<lb n="14" xml:id="Gf15v.14"/><l n="G_KE_0774"><hi rend="capitalsize1">W</hi>ant et ontuyel mir vp der <choice><abbr>straete<g ref="#bar"/></abbr><expan>straete<ex>n</ex></expan></choice></l>
<lb n="15" xml:id="Gf15v.15"/><l n="G_KE_0775"><hi rend="capitalsize1">D</hi>o volghede mir nae der wachter</l>
<lb n="16" xml:id="Gf15v.16"/><l n="G_KE_0776"><hi rend="capitalsize1">I</hi>n dorste mi nit <choice><abbr>o<g ref="#bar"/>me</abbr><expan>o<ex>m</ex>me</expan></choice> <choice><abbr>keere<g ref="#bar"/></abbr><expan>keere<ex>n</ex></expan></choice> achter</l>
<lb n="17" xml:id="Gf15v.17"/><l n="G_KE_0777"><hi rend="capitalsize1">D</hi>us bin ich mins ysers ane</l>
<lb n="18" xml:id="Gf15v.18"/><l n="G_KE_0778"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> ich nam bi den mane</l>
<lb n="19" xml:id="Gf15v.19"/><l n="G_KE_0779"><hi rend="capitalsize1">D</hi>ar icht vaent an einen pluch</l>
<lb n="20" xml:id="Gf15v.20"/><l n="G_KE_0780"><hi rend="capitalsize1">D</hi>o stunt elegast <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> luch</l>
<lb n="21" xml:id="Gf15v.21"/><l n="G_KE_0781"><hi rend="capitalsize1">M</hi>ogen wir hie te punten <choice><abbr>geracke<g ref="#bar"/></abbr><expan>geracke<ex>n</ex></expan></choice></l>
<lb n="22" xml:id="Gf15v.22"/><l n="G_KE_0782"><choice><abbr><hi rend="capitalsize1">H</hi><damage><gap></gap></damage>ke<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">H</hi><damage><gap></gap></damage>ke<ex>n</ex></expan></choice></l>


<pb xml:id="G.f16r" n="16r"/>
<lb n="1" xml:id="Gf16r.1"/><l n="G_KE_0786"><hi rend="capitalsize1">D</hi>at <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> karle deede</l>
<lb n="2" xml:id="Gf16r.2"/><l n="G_KE_0787"><hi rend="capitalsize1">A</hi>l was he groet <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> steerc</l>
<lb n="3" xml:id="Gf16r.3"/><l n="G_KE_0788"><hi rend="capitalsize1">H</hi>e in conste nit al sullich weerck</l>
<lb n="4" xml:id="Gf16r.4"/><l n="G_KE_0789"><hi rend="capitalsize2">D</hi>o si dat gaet <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> der muren</l>
<lb n="5" xml:id="Gf16r.5"/><l n="G_KE_0790"><hi rend="capitalsize1">H</hi>adden gebracht all duren</l>
<lb n="6" xml:id="Gf16r.6"/><l n="G_KE_0791"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> si <choice><abbr>binne<g ref="#bar"/></abbr><expan>binne<ex>n</ex></expan></choice> souden gaen</l>
<lb n="7" xml:id="Gf16r.7"/><l n="G_KE_0792"><hi rend="capitalsize1">S</hi>eide elegast. ir sult ontfaen</l>
<lb n="8" xml:id="Gf16r.8"/><l n="G_KE_0793"><hi rend="capitalsize1">H</hi>ir buten dat ich v. sal bringen</l>
<lb n="9" xml:id="Gf16r.9"/><l n="G_KE_0794"><hi rend="capitalsize1">W</hi>ant ich in wijl nit gehingen</l>
<lb n="10" xml:id="Gf16r.10"/><l n="G_KE_0795"><hi rend="capitalsize1">D</hi>at ir dar <choice><abbr>binne<g ref="#bar"/></abbr><expan>binne<ex>n</ex></expan></choice> quame</l>
<lb n="11" xml:id="Gf16r.11"/><l n="G_KE_0796"><hi rend="capitalsize1">I</hi>t dochte home ein schame</l>
<lb n="12" xml:id="Gf16r.12"/><l n="G_KE_0797"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> et dochte home eghein behendich dijef</l>
<lb n="13" xml:id="Gf16r.13"/><l n="G_KE_0798"><hi rend="capitalsize1">N</hi>ochtan woude he lief <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> leit</l>
<lb n="14" xml:id="Gf16r.14"/><l n="G_KE_0799"><hi rend="capitalsize1">M</hi>it hom deilen sin gewijn</l>
<lb n="15" xml:id="Gf16r.15"/><l n="G_KE_0800"><hi rend="capitalsize1">D</hi>ie <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> bleif buten. elegast ghijnc in</l>
<lb n="16" xml:id="Gf16r.16"/><l n="G_KE_0801"><hi rend="capitalsize1">M</hi>ar elegasts behendicheide</l>
<lb n="17" xml:id="Gf16r.17"/><l n="G_KE_0802"><hi rend="capitalsize1">D</hi>ie he prueft te <choice><abbr>me<g ref="#bar"/>niger</abbr><expan>me<ex>n</ex>niger</expan></choice> steede</l>
<lb n="18" xml:id="Gf16r.18"/><l n="G_KE_0803"><hi rend="capitalsize1">D</hi>ie was menlich. <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> gemaete</l>
<lb n="19" xml:id="Gf16r.19"/><l n="G_KE_0804"><hi rend="capitalsize1">H</hi>e traect ein cruyt vt <choice><abbr>sine<g ref="#bar"/></abbr><expan>sine<ex>n</ex></expan></choice> vate</l>
<lb n="20" xml:id="Gf16r.20"/><l n="G_KE_0805"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> staect home in <choice><abbr>sine<g ref="#bar"/></abbr><expan>sine<ex>n</ex></expan></choice> monde</l>
<lb n="21" xml:id="Gf16r.21"/><l n="G_KE_0806"><hi rend="capitalsize1">D</hi>at si alle <choice><abbr>verstu<g ref="#bar"/>nden</abbr><expan>verstu<ex>e</ex>nden</expan></choice></l>
<lb n="22" xml:id="Gf16r.22"/><l n="G_KE_0807"><damage><gap></gap></damage></l>


<pb xml:id="G.f16v" n="16v"/>
<lb n="1" xml:id="Gf16v.1"/><l n="G_KE_0811"><hi rend="capitalsize1">B</hi>uten houe in haer latijn</l>
<lb n="2" xml:id="Gf16v.2"/><l n="G_KE_0812"><hi rend="capitalsize1">E</hi>legast. seide wat sal dit sijn</l>
<lb n="3" xml:id="Gf16v.3"/><l n="G_KE_0813"><hi rend="capitalsize1">S</hi>oude die <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> sijn hier voren</l>
<lb n="4" xml:id="Gf16v.4"/><l n="G_KE_0814"><hi rend="capitalsize1">S</hi>o naecket my groet toren</l>
<lb n="5" xml:id="Gf16v.5"/><l n="G_KE_0815"><hi rend="capitalsize1">I</hi>ch ben verroeden in min gedochte</l>
<lb n="6" xml:id="Gf16v.6"/><l n="G_KE_0816"><hi rend="capitalsize1">O</hi>f et es half gedrochte</l>
<lb n="7" xml:id="Gf16v.7"/><l n="G_KE_0817"><hi rend="capitalsize1">H</hi>e ghijnc do he den <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> liet</l>
<lb n="8" xml:id="Gf16v.8"/><l n="G_KE_0818"><hi rend="capitalsize1">T</hi>er steede do he <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> home schiet</l>
<lb n="9" xml:id="Gf16v.9"/><l n="G_KE_0819"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> seide wat he hadde verstaen</l>
<lb n="10" xml:id="Gf16v.10"/><l n="G_KE_0820"><hi rend="capitalsize1">O</hi>f hom <choice><abbr>bedroge<g ref="#bar"/></abbr><expan>bedroge<ex>n</ex></expan></choice> hadde sin waen</l>
<lb n="11" xml:id="Gf16v.11"/><l n="G_KE_0825"><hi rend="capitalsize1">D</hi>at die <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> ware dare</l>
<lb n="12" xml:id="Gf16v.12"/><l n="G_KE_0826"><hi rend="capitalsize1">M</hi>aer he in west nit wie nare</l>
<lb n="13" xml:id="Gf16v.13"/><l n="G_KE_0823"><hi rend="capitalsize1">B</hi>eide an hanen. <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> an hunden</l>
<lb n="14" xml:id="Gf16v.14"/><l n="G_KE_0824"><hi rend="capitalsize1">S</hi>i dit in haer latijn verorcunden</l>
<lb n="15" xml:id="Gf16v.15"/><l n="G_KE_0827"><hi rend="capitalsize2">D</hi>o sprach <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> karle die edel man</l>
<lb n="16" xml:id="Gf16v.16"/><l n="G_KE_0828"><hi rend="capitalsize1">W</hi>ie heet v. gesacht dan</l>
<lb n="17" xml:id="Gf16v.17"/><l n="G_KE_0829"><hi rend="capitalsize1">W</hi>at soude die <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> hir doen</l>
<lb n="18" xml:id="Gf16v.18"/><l n="G_KE_0830"><hi rend="capitalsize1">S</hi>oudi gelouuen an ein huen</l>
<lb n="19" xml:id="Gf16v.19"/><l n="G_KE_0831"><hi rend="capitalsize1">O</hi>f an einen hunt die baest</l>
<lb n="20" xml:id="Gf16v.20"/><l n="G_KE_0832"><hi rend="capitalsize1">S</hi>o is vr gelouue nit vast</l>

<note n="G_KE_0833">Line_number 0833 is missing</note>

<note n="G_KE_0834">Line_number 0834 is missing</note>

<note n="G_KE_0835">Line_number 0835 is missing</note>

<note n="G_KE_0836">Line_number 0836 is missing</note>
<lb n="21" xml:id="Gf16v.21"/><l n="G_KE_0837"><hi rend="capitalsize1">H</hi>e staeck <choice><abbr>de<g ref="#bar"/></abbr><expan>de<ex>n</ex></expan></choice> <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> in <choice><abbr>sine<g ref="#bar"/></abbr><expan>sine<ex>n</ex></expan></choice> mont</l>
<lb n="22" xml:id="Gf16v.22"/><l n="G_KE_0838"><damage><gap></gap></damage></l>


<pb xml:id="G.f22r" n="22r"/>
<lb n="1" xml:id="Gf22r.1"/><l n="G_KE_1105"><hi rend="capitalsize1">S</hi>in hertte in was nit sunder twijuel</l>
<lb n="2" xml:id="Gf22r.2"/><l n="G_KE_1106"><hi rend="capitalsize1">D</hi>at hom elegast woude verraden</l>
<lb n="3" xml:id="Gf22r.3"/><l n="G_KE_1107"><hi rend="capitalsize1">D</hi>ie home soude by staen in raden</l>
<lb n="4" xml:id="Gf22r.4"/><l n="G_KE_1108"><hi rend="capitalsize1">S</hi>oude recht. na rechte gaen</l>
<lb n="5" xml:id="Gf22r.5"/><l n="G_KE_1109"><hi rend="capitalsize1">D</hi>ie portte vaent he vp gedaen</l>

<note n="G_KE_1110">Line_number 1110 is missing</note>

<note n="G_KE_1111">Line_number 1111 is missing</note>

