<p>

<pb xml:id="M.f1ra" n="1ra"/>
<lb n="1" xml:id="Mf1ra.1"/><l n="M_KE_0001"><hi rend="capitalsize7">V</hi>Raye ystorie <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> al waer</l>
<lb n="2" xml:id="Mf1ra.2"/><l n="M_KE_0002"><hi rend="capitalsize1">M</hi>agic v tellen horter naer</l>
<lb n="3" xml:id="Mf1ra.3"/><l n="M_KE_0003"><hi rend="capitalsize1">H</hi>et was op ene <choice><abbr>auo<g ref="#bar"/>stonde</abbr><expan>auo<ex>n</ex>stonde</expan></choice></l>
<lb n="4" xml:id="Mf1ra.4"/><l n="M_KE_0004"><hi rend="capitalsize1">D</hi>at karel <choice><abbr>slape<g ref="#bar"/>de</abbr><expan>slape<ex>n</ex>de</expan></choice> begonde</l>
<lb n="5" xml:id="Mf1ra.5"/><l n="M_KE_0005"><hi rend="capitalsize1">T</hi>inglem op den rijn</l>
<lb n="6" xml:id="Mf1ra.6"/><l n="M_KE_0006"><hi rend="capitalsize1">D</hi>lant was al gader sijn</l>
<lb n="7" xml:id="Mf1ra.7"/><l n="M_KE_0007"><hi rend="capitalsize1">H</hi>i was keyser <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> mede</l>
<lb n="8" xml:id="Mf1ra.8"/><l n="M_KE_0008"><hi rend="capitalsize1">H</hi>oort hier <choice><abbr>wond<g ref="#apomod"/></abbr><expan>wond<ex>er</ex></expan></choice> <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> <choice><abbr>w<g ref="#apomod"/>hede</abbr><expan>w<ex>aer</ex>hede</expan></choice></l>
<lb n="9" xml:id="Mf1ra.9"/><l n="M_KE_0009"><hi rend="capitalsize1">W</hi>at den <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> daer gheuel</l>
<lb n="10" xml:id="Mf1ra.10"/><l n="M_KE_0010"><hi rend="capitalsize1">D</hi>at <choice><abbr>wete<g ref="#bar"/></abbr><expan>wete<ex>n</ex></expan></choice> noch die menege wel</l>
<lb n="11" xml:id="Mf1ra.11"/><l n="M_KE_0011"><hi rend="capitalsize1">T</hi>inglem al daer hi lach</l>
<lb n="12" xml:id="Mf1ra.12"/><l n="M_KE_0012"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> waende opten <choice><abbr>andre<g ref="#bar"/></abbr><expan>andre<ex>n</ex></expan></choice> dach</l>
<lb n="13" xml:id="Mf1ra.13"/><l n="M_KE_0013"><hi rend="capitalsize1">C</hi>rone <choice><abbr>drage<g ref="#bar"/></abbr><expan>drage<ex>n</ex></expan></choice> <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> <choice><abbr>houde<g ref="#bar"/></abbr><expan>houde<ex>n</ex></expan></choice> hof</l>
<lb n="14" xml:id="Mf1ra.14"/><l n="M_KE_0014"><choice><abbr><hi rend="capitalsize1">O</hi><g ref="#bar"/>me</abbr><expan><hi rend="capitalsize1">O</hi><ex>m</ex>me</expan></choice> te meerne sinen lof</l>
<lb n="15" xml:id="Mf1ra.15"/><l n="M_KE_0015"><choice><abbr><hi rend="capitalsize1">D</hi><g ref="#apomod"/></abbr><expan><hi rend="capitalsize1">D</hi><ex>aer</ex></expan></choice> die <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> lach <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> en sliep</l>
<lb n="16" xml:id="Mf1ra.16"/><l n="M_KE_0016"><hi rend="capitalsize1">E</hi>en heylich ingel aen <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> riep</l>
<lb n="17" xml:id="Mf1ra.17"/><l n="M_KE_0017"><hi rend="capitalsize1">S</hi>oe dat die <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> ontbrac</l>
<lb n="18" xml:id="Mf1ra.18"/><l n="M_KE_0018"><choice><abbr><hi rend="capitalsize1">B</hi>ide<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">B</hi>ide<ex>n</ex></expan></choice> woorde die dingel sprac</l>
<lb n="19" xml:id="Mf1ra.19"/><l n="M_KE_0019"><hi rend="capitalsize1">H</hi>i seide staet op edel man</l>
<lb n="20" xml:id="Mf1ra.20"/><l n="M_KE_0020"><hi rend="capitalsize1">D</hi>oet <choice><abbr>haestelike<g ref="#bar"/></abbr><expan>haestelike<ex>n</ex></expan></choice> v cleder an</l>
<lb n="21" xml:id="Mf1ra.21"/><l n="M_KE_0021">wapent v <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> vart stelen</l>
<lb n="22" xml:id="Mf1ra.22"/><l n="M_KE_0022"><hi rend="capitalsize1">G</hi>od hiet mi v beuelen</l>
<lb n="23" xml:id="Mf1ra.23"/><l n="M_KE_0023"><hi rend="capitalsize1">D</hi>ie in hemelrike es here</l>
<lb n="24" xml:id="Mf1ra.24"/><l n="M_KE_0024"><hi rend="capitalsize1">O</hi>ft ghi verliest lijf ende ere</l>
<lb n="25" xml:id="Mf1ra.25"/><l n="M_KE_0025"><hi rend="capitalsize1">E</hi>n steeldi in deser nacht niet</l>
<lb n="26" xml:id="Mf1ra.26"/><l n="M_KE_0026"><hi rend="capitalsize1">S</hi>oe es v euel ghesciet</l>
<lb n="27" xml:id="Mf1ra.27"/><l n="M_KE_0027"><hi rend="capitalsize1">G</hi>hi selter <choice><abbr>o<g ref="#bar"/>me</abbr><expan>o<ex>m</ex>me</expan></choice> steruen</l>
<lb n="28" xml:id="Mf1ra.28"/><l n="M_KE_0028"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> uwes liues deruen</l>
<lb n="29" xml:id="Mf1ra.29"/><l n="M_KE_0029"><hi rend="capitalsize1">E</hi>er <choice><abbr>e<g ref="#bar"/>mer</abbr><expan>e<ex>m</ex>mer</expan></choice> sceedt dit hof</l>
<lb n="30" xml:id="Mf1ra.30"/><l n="M_KE_0030"><hi rend="capitalsize1">N</hi>v <choice><abbr>v<g ref="#apomod"/>wacht</abbr><expan>v<ex>er</ex>wacht</expan></choice> v daer of</l>
<lb n="31" xml:id="Mf1ra.31"/><l n="M_KE_0031"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> vart stelen oft ghi wilt</l>
<lb n="32" xml:id="Mf1ra.32"/><l n="M_KE_0032"><hi rend="capitalsize1">N</hi>emt <choice><abbr>uwe<g ref="#bar"/></abbr><expan>uwe<ex>n</ex></expan></choice> spere <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> <choice><abbr>uwe<g ref="#bar"/></abbr><expan>uwe<ex>n</ex></expan></choice> scilt</l>
<lb n="33" xml:id="Mf1ra.33"/><l n="M_KE_0033">wapent v <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> sit op v part</l>
<lb n="34" xml:id="Mf1ra.34"/><l n="M_KE_0034"><choice><abbr><hi rend="capitalsize1">H</hi>aestelike<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">H</hi>aestelike<ex>n</ex></expan></choice> <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> niet en spaert</l>
<lb n="35" xml:id="Mf1ra.35"/><l n="M_KE_0035"><hi rend="capitalsize2">D</hi>it verhoorde die .coninc.</l>
<lb n="36" xml:id="Mf1ra.36"/><l n="M_KE_0036"><hi rend="capitalsize1">H</hi>et dochte <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> <choice><abbr>z<g ref="#apomod"/></abbr><expan>z<ex>eer</ex></expan></choice> vremde dinc</l>
<lb n="37" xml:id="Mf1ra.37"/><l n="M_KE_0037">want hi <choice><abbr>d<g ref="#apomod"/></abbr><expan>d<ex>aer</ex></expan></choice> <choice><abbr>nieme<g ref="#bar"/></abbr><expan>nieme<ex>n</ex></expan></choice> en sach</l>
<lb n="38" xml:id="Mf1ra.38"/><l n="M_KE_0038">wat dat <choice><abbr>roepe<g ref="#bar"/></abbr><expan>roepe<ex>n</ex></expan></choice> bedieden mach</l>

<note n="M_KE_0039">Line_number 0039 is missing</note>

<note n="M_KE_0040">Line_number 0040 is missing</note>
<lb n="39" xml:id="Mf1ra.39"/><l n="M_KE_0041"><hi rend="capitalsize1">H</hi>i was in <choice><abbr>menege<g ref="#bar"/></abbr><expan>menege<ex>n</ex></expan></choice> ghedochte</l>
<lb n="40" xml:id="Mf1ra.40"/><l n="M_KE_0042">wie <choice><abbr>d<g ref="#bar"/></abbr><expan>d<ex>aer</ex></expan></choice> die boetscap brochte</l>
<lb n="41" xml:id="Mf1ra.41"/><l n="M_KE_0043"><hi rend="capitalsize1">D</hi>ingel die <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> gode quam</l>


<pb xml:id="M.f1rb" n="1rb"/>
<lb n="1" xml:id="Mf1rb.1"/><l n="M_KE_0044"><hi rend="capitalsize1">S</hi>prac ten <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> als die was g<damage><gap></gap></damage></l>
<lb n="2" xml:id="Mf1rb.2"/><l n="M_KE_0045"><hi rend="capitalsize1">S</hi>taet op ka<damage><gap></gap></damage>l <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> vart s<damage><gap></gap></damage></l>
<lb n="3" xml:id="Mf1rb.3"/><l n="M_KE_0046"><hi rend="capitalsize1">G</hi>od hiet mi v beuelen</l>
<lb n="4" xml:id="Mf1rb.4"/><l n="M_KE_0047"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> ontbiedet v te voren</l>
<lb n="5" xml:id="Mf1rb.5"/><l n="M_KE_0048"><hi rend="capitalsize1">O</hi>ft anders hebdi v lijf v<damage><gap></gap></damage></l>
<lb n="6" xml:id="Mf1rb.6"/><l n="M_KE_0049"><hi rend="capitalsize2">M</hi>Ettien woorde sweech <damage><gap></gap></damage></l>
<lb n="7" xml:id="Mf1rb.7"/><l n="M_KE_0050"><hi rend="capitalsize1">E</hi>ntie <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> riep ay mi</l>
<lb n="8" xml:id="Mf1rb.8"/><l n="M_KE_0051"><hi rend="capitalsize1">A</hi>ls die sere was vereen<damage><gap></gap></damage></l>
<lb n="9" xml:id="Mf1rb.9"/><l n="M_KE_0052">wat es dat dit <choice><abbr>wond<g ref="#apomod"/></abbr><expan>wond<ex>er</ex></expan></choice> mee<damage><gap></gap></damage></l>
<lb n="10" xml:id="Mf1rb.10"/><l n="M_KE_0053"><hi rend="capitalsize1">E</hi>est alfs ghedrochte dat <damage><gap></gap></damage></l>
<lb n="11" xml:id="Mf1rb.11"/><l n="M_KE_0054"><hi rend="capitalsize1">E</hi>nde dit grote <choice><abbr>wond<g ref="#apomod"/></abbr><expan>wond<ex>er</ex></expan></choice> telt</l>
<lb n="12" xml:id="Mf1rb.12"/><l n="M_KE_0055"><hi rend="capitalsize1">A</hi>y hemelsche drochtij<damage><gap></gap></damage></l>
<lb n="13" xml:id="Mf1rb.13"/><l n="M_KE_0056">wat node soude mi sijn</l>
<lb n="14" xml:id="Mf1rb.14"/><l n="M_KE_0057"><hi rend="capitalsize1">T</hi>e stelene ic ben soe rik<damage><gap></gap></damage></l>
<lb n="15" xml:id="Mf1rb.15"/><l n="M_KE_0058"><hi rend="capitalsize1">E</hi>n es <choice><abbr>ma<g ref="#bar"/></abbr><expan>ma<ex>n</ex></expan></choice> in erterike</l>
<lb n="16" xml:id="Mf1rb.16"/><l n="M_KE_0059"><choice><abbr>wed<g ref="#apomod"/></abbr><expan>wed<ex>er</ex></expan></choice> <choice><abbr>co<g ref="#bar"/></abbr><expan>co<ex>ninc</ex></expan></choice> no graue</l>
<lb n="17" xml:id="Mf1rb.17"/><l n="M_KE_0060"><hi rend="capitalsize1">D</hi>ie soe rijc sijn <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> <choice><abbr>haue<g ref="#bar"/></abbr><expan>haue<ex>n</ex></expan></choice></l>
<lb n="18" xml:id="Mf1rb.18"/><l n="M_KE_0061"><hi rend="capitalsize1">S</hi>ine <choice><abbr>moete<g ref="#bar"/></abbr><expan>moete<ex>n</ex></expan></choice> mi sijn <choice><abbr>ond<g ref="#apomod"/><damage><gap></gap></damage></abbr><expan>ond<ex>er</ex><damage><gap></gap></damage></expan></choice></l>
<lb n="19" xml:id="Mf1rb.19"/><l n="M_KE_0062"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> te <choice><abbr>mine<g ref="#bar"/></abbr><expan>mine<ex>n</ex></expan></choice> dienste stae<damage><gap></gap></damage></l>
<lb n="20" xml:id="Mf1rb.20"/><l n="M_KE_0063"><hi rend="capitalsize1">M</hi>ijns selfs lant es soe g<damage><gap></gap></damage></l>
<lb n="21" xml:id="Mf1rb.21"/><l n="M_KE_0064"><hi rend="capitalsize1">M</hi>en vint n<damage><gap></gap></damage>s s<damage><gap></gap></damage></l>
<lb n="22" xml:id="Mf1rb.22"/><l n="M_KE_0065"><hi rend="capitalsize1">D</hi>lant es al ga<damage><gap></gap></damage> m<damage><gap></gap></damage></l>
<lb n="23" xml:id="Mf1rb.23"/><l n="M_KE_0066"><hi rend="capitalsize1">T</hi>ote coelne op<damage><gap></gap></damage></l>
<lb n="24" xml:id="Mf1rb.24"/><l n="M_KE_0067"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> tote rome al<damage><gap></gap></damage></l>
<lb n="25" xml:id="Mf1rb.25"/><l n="M_KE_0068"><hi rend="capitalsize1">A</hi>ls den keyser toe b<damage><gap></gap></damage></l>
<lb n="26" xml:id="Mf1rb.26"/><l n="M_KE_0069"><hi rend="capitalsize2">I</hi>C ben <choice><abbr>h<g ref="#apomod"/>e</abbr><expan>h<ex>er</ex>e</expan></choice> mine <damage><gap></gap></damage>ijf<damage><gap></gap></damage></l>
<lb n="27" xml:id="Mf1rb.27"/><l n="M_KE_0070"><hi rend="capitalsize1">O</hi>est toter wi<damage><gap></gap></damage> dyno</l>
<lb n="28" xml:id="Mf1rb.28"/><l n="M_KE_0071"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> wes <choice><abbr>tot<g ref="#apomod"/></abbr><expan>tot<ex>er</ex></expan></choice> wilder see</l>
<lb n="29" xml:id="Mf1rb.29"/><l n="M_KE_0072"><hi rend="capitalsize1">N</hi>ochtan hebbic goets <damage><gap></gap></damage></l>
<lb n="30" xml:id="Mf1rb.30"/><l n="M_KE_0073"><choice><abbr><hi rend="capitalsize1">G</hi>a<damage><gap></gap></damage>issie<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">G</hi>a<damage><gap></gap></damage>issie<ex>n</ex></expan></choice> <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> <choice><abbr>spaendie<g ref="#bar"/></abbr><expan>spaendie<ex>n</ex></expan></choice> la<damage><gap></gap></damage></l>
<lb n="31" xml:id="Mf1rb.31"/><l n="M_KE_0074"><hi rend="capitalsize1">D</hi>at ic wan met mier<damage><gap></gap></damage></l>
<lb n="32" xml:id="Mf1rb.32"/><l n="M_KE_0075"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> ic die heidene <choice><abbr>v<g ref="#apomod"/>dre<damage><gap></gap></damage></abbr><expan>v<ex>er</ex>dre<damage><gap></gap></damage></expan></choice></l>
<lb n="33" xml:id="Mf1rb.33"/><l n="M_KE_0076"><hi rend="capitalsize1">S</hi>oe dat mi dlant alle<damage><gap></gap></damage></l>
<lb n="34" xml:id="Mf1rb.34"/><l n="M_KE_0077">wat node <unclear>so</unclear>ude mi wes<damage><gap></gap></damage></l>
<lb n="35" xml:id="Mf1rb.35"/><l n="M_KE_0078"><hi rend="capitalsize1">T</hi>e varne stelen ellen<damage><gap></gap></damage></l>
